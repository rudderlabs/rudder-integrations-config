{
  "name": "GOOGLE_ADWORDS_ENHANCED_CONVERSIONS",
  "displayName": "Google Ads Enhanced Conversions",
  "config": {
    "auth": {
      "type": "OAuth",
      "provider": "Google",
      "role": "google_adwords_enhanced_conversions_v1",
      "rudderScopes": ["delivery"]
    },
    "transformAt": "router",
    "transformAtV1": "router",
    "saveDestinationResponse": true,
    "includeKeys": ["oneTrustCookieCategories"],
    "excludeKeys": [],
    "supportedConnectionModes": {
      "web": ["cloud"],
      "android": ["cloud"],
      "ios": ["cloud"],
      "unity": ["cloud"],
      "amp": ["cloud"],
      "reactnative": ["cloud"],
      "flutter": ["cloud"],
      "cordova": ["cloud"],
      "shopify": ["cloud"]
    },
    "supportedMessageTypes": { "cloud": ["track"] },
    "supportedSourceTypes": [
      "android",
      "ios",
      "web",
      "unity",
      "amp",
      "cloud",
      "reactnative",
      "flutter",
      "cordova",
      "warehouse",
      "shopify"
    ],
    "destConfig": {
      "defaultConfig": [
        "rudderAccountId",
        "customerId",
        "listOfConversions",
        "subAccount",
        "loginCustomerId",
        "requireHash",
        "oneTrustCookieCategories"
      ]
    }
  },
  "options": { "isBeta": false }
}
