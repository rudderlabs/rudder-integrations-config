    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": ["host", "port", "path", "token", "syncFrequency", "bucketProvider"],
    "properties": {
      "host": {
        "type": "string",
        "pattern": "(^env[.].+)|(?!.*\\.ngrok\\.io)^(.{1,100})$"
      },
      "port": {
        "type": "string",
        "pattern": "(^env[.].+)|^(.{1,100})$"
      },
      "path": {
        "type": "string",
        "pattern": "(^env[.].+)|^(.{1,100})$"
      },
      "token": {
        "type": "string",
        "pattern": "(^env[.].+)|^(.{1,100})$"
      },
      "catalog": {
        "type": "string",
        "pattern": "(^env[.].*)|^(.*)$"
      },
      "namespace": {
        "type": "string",
        "pattern": "(^env[.].*)|^((?!pg_|PG_|pG_|Pg_).{0,64})$"
      },
      "skipTracksTable": {
        "type": "boolean",
        "default": false
      },
      "skipUsersTable": {
        "type": "boolean",
        "default": false
      },
      "syncFrequency": {
        "type": "string",
        "pattern": "^(30|60|180|360|720|1440)$",
        "default": "180"
      },
      "syncStartAt": {
        "type": "string"
      },
      "excludeWindow": {
        "type": "object",
        "required": ["excludeWindowStartTime", "excludeWindowEndTime"],
        "properties": {
          "excludeWindowStartTime": {
            "type": "string"
          },
          "excludeWindowEndTime": {
            "type": "string"
          }
        }
      },
      "prefix": {
        "type": "string",
        "pattern": "(^env[.].*)|^(.{0,100})$"
      },
      "bucketProvider": {
        "type": "string",
        "pattern": "^(S3|GCS|AZURE_BLOB)$"
      },
      "enableExternalLocation": {
        "type": "boolean",
        "default": false
      },
      "useRudderStorage": {
        "type": "boolean",
        "default": false
      },
      "preferAppend": {
        "type": "boolean",
        "default": true
      },
      "oneTrustCookieCategories": {
