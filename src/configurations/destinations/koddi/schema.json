{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "required": ["apiBaseUrl", "clientName"],
    "type": "object",
    "properties": {
      "apiBaseUrl": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|(?!.*\\.ngrok\\.io)^(.{0,100})$"
      },
      "clientName": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|(?!.*\\.ngrok\\.io)^(.{0,100})$"
      },
      "testVersionOverride": {
        "type": "boolean",
        "default": false
      },
      "overrides": {
        "type": "boolean",
        "default": false
      },
      "connectionMode": {
        "type": "object",
        "properties": {
          "android": {
            "type": "string",
            "enum": ["cloud"]
          },
          "ios": {
            "type": "string",
            "enum": ["cloud"]
          },
          "web": {
            "type": "string",
            "enum": ["cloud"]
          },
          "unity": {
            "type": "string",
            "enum": ["cloud"]
          },
          "amp": {
            "type": "string",
            "enum": ["cloud"]
          },
          "cloud": {
            "type": "string",
            "enum": ["cloud"]
          },
          "reactnative": {
            "type": "string",
            "enum": ["cloud"]
          },
          "flutter": {
            "type": "string",
            "enum": ["cloud"]
          },
          "cordova": {
            "type": "string",
            "enum": ["cloud"]
          },
          "shopify": {
            "type": "string",
            "enum": ["cloud"]
          }
        }
      }
    }
  }
}
