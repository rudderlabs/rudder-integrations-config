    {
      "title": "Consent Settings",
      "fields": [
        {
          "type": "dynamicCustomForm",
          "value": "consentManagement",
          "label": "Consent management settings",
          "footerNote": "We recommend using IDs instead of names as IDs are unique and less likely to change over time, making them a more reliable choice.",
          "customFields": [
            {
              "type": "singleSelect",
              "label": "Consent management provider",
              "value": "provider",
              "options": [
                {
                  "name": "Custom",
                  "value": "custom"
                },
                {
                  "name": "Ketch",
                  "value": "ketch"
                },
                {
                  "name": "OneTrust",
                  "value": "oneTrust"
                }
              ],
              "defaultOption": {
                "name": "OneTrust",
                "value": "oneTrust"
              },
              "required": true
            },
            {
              "type": "singleSelect",
              "label": "the required consent logic",
              "value": "resolutionStrategy",
              "options": [
                {
                  "name": "AND",
                  "value": "and"
                },
                {
                  "name": "OR",
                  "value": "or"
                }
              ],
              "required": true,
              "variant": "badge",
              "preRequisites": {
                "fields": [
                  {
                    "configKey": "provider",
                    "value": "custom"
                  }
                ]
              }
            },
            {
              "type": "dynamicCustomForm",
              "value": "consents",
              "label": "Enter consent category IDs",
              "customFields": [
                {
                  "type": "textInput",
                  "placeholder": "Marketing",
                  "value": "consent",
                  "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
                  "required": false
                }
              ]
            }
          ]
        }
      ]
    }
