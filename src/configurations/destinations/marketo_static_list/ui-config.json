{
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "Munchkin Account Id",
          "value": "accountId",
          "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
          "regexErrorMessage": "Invalid Munchkin Account Id",
          "secret": true,
          "required": true,
          "footerNote": "Your Marketo Accound ID from Admin settings section",
          "placeholder": "e.g: 585-AXP-446"
        },
        {
          "type": "textInput",
          "label": "Client ID",
          "value": "clientId",
          "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
          "regexErrorMessage": "Invalid Client ID",
          "secret": true,
          "required": true,
          "placeholder": "e.g: 53b1934e-51dd-4599-b24b-92612c71515f",
          "footerNote": "Your REST API Client ID"
        },
        {
          "type": "textInput",
          "label": "Client Secret",
          "value": "clientSecret",
          "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
          "regexErrorMessage": "Invalid Client Secret",
          "required": true,
          "placeholder": "e.g: oxzXtNhcuw9YGsdhplKUeEMi8765dsfw",
          "secret": true,
          "footerNote": "Your REST API Client Secret"
        },
        {
          "type": "textInput",
          "label": "Static List ID",
          "value": "staticListId",
          "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
          "regexErrorMessage": "Invalid Client Secret",
          "required": true,
          "placeholder": "e.g: 3027",
          "secret": false,
          "footerNote": "This Static list Id would be used as a default Static List Id (if not provided in externalId) to add/remove leads to/from. "
        }
      ]
    }
  ]
}
