{
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "Munchkin Account Id",
          "value": "accountId",
          "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
          "regexErrorMessage": "Invalid Munchkin Account Id",
          "secret": true,
          "required": true,
          "footerNote": "Your Marketo Accound ID from Admin settings section",
          "placeholder": "e.g: 5X5-AXP-4X6"
        },
        {
          "type": "textInput",
          "label": "Client ID",
          "value": "clientId",
          "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
          "regexErrorMessage": "Invalid Client ID",
          "secret": true,
          "required": true,
          "placeholder": "e.g: 53b1934e-XXXX-XXXX-XXXX-92612c71515f",
          "footerNote": "Your REST API Client ID"
        },
        {
          "type": "textInput",
          "label": "Client Secret",
          "value": "clientSecret",
          "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
          "regexErrorMessage": "Invalid Client Secret",
          "required": true,
          "placeholder": "e.g: oxzXtNhcuw9YGXXXXlKUeEMi8765dsfw",
          "secret": true,
          "footerNote": "Your REST API Client Secret"
        },
        {
          "type": "textInput",
          "label": "Static List ID",
          "value": "staticListId",
          "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
          "regexErrorMessage": "Invalid Client Secret",
          "required": true,
          "placeholder": "e.g: 30X7",
          "secret": false,
          "footerNote": "This Static list Id would be used as a default Static List Id (if not provided in externalId) to add/remove leads to/from. "
        }
      ]
    },
    {
      "title": "Consent Settings",
      "fields": [
        {
          "type": "dynamicCustomForm",
          "value": "oneTrustCookieCategories",
          "label": "OneTrust Consent Category IDs",
          "footerNote": "The support for category names is deprecated. We recommend using the category IDs instead of the names as IDs are unique and less likely to change over time, making them a more reliable choice.",
          "customFields": [
            {
              "type": "textInput",
              "placeholder": "C0001",
              "value": "oneTrustCookieCategory",
              "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
              "label": "Category ID",
              "required": false
            }
          ]
        },
        {
          "type": "dynamicCustomForm",
          "value": "ketchConsentPurposes",
          "label": "Ketch Consent Purpose IDs",
          "customFields": [
            {
              "type": "textInput",
              "placeholder": "marketing",
              "value": "purpose",
              "label": "Purpose ID",
              "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
              "required": false
            }
          ]
        }
      ]
    }
  ]
}
