{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["cloudMode"],
    "properties": {
      "cloudMode": {
        "type": "string",
        "pattern": "^(web|server)$"
      }
    },
    "anyOf": [
      {
        "properties": {
          "pl": {
            "type": "string",
            "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|(?!.*\\.ngrok\\.io)^(https?|ftp)://[^\\s/$.?#].[^\\s]*$"
          },
          "plWriteKey": {
            "type": "string",
            "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
          }
        },
        "required": ["pl","plWriteKey"]
      },
      {
        "properties": {
          "apiKey": {
            "type": "string",
            "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
          },
          "passKey": {
            "type": "string",
            "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
          },
          "diapi": {
            "type": "string",
            "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|(?!.*\\.ngrok\\.io)^(https?|ftp)://[^\\s/$.?#].[^\\s]*$"
          },
          "srcId": {
            "type": "string",
            "pattern": "(^env[.].+)|^(.+)$"
          },
          "diapiWriteKey": {
            "type": "string",
            "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
          }
        },
        "required": ["apiKey", "passKey","diapi"]
      }
    ]
  }
}
