{
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "API Key",
          "value": "apiKey",
          "regex": "^(.{0,61})$",
          "regexErrorMessage": "Invalid API Key",
          "required": true,
          "placeholder": "e.g: Tudc...5G4z"
        },
        {
          "type": "checkbox",
          "label": "Fall back to anonymousId if userId is not present",
          "value": "sendAnonymousId",
          "default": false
        }
      ]
    }
  ]
}