            "default": "oneTrust"
          },
          "consents": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "consent": {
                  "type": "string",
                  "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
                }
              }
            }
          }
        },
        "allOf": [
          {
            "if": {
              "properties": {
                "provider": {
                  "const": "custom"
                }
              },
              "required": ["provider"]
            },
            "then": {
              "properties": {
                "resolutionStrategy": {
                  "type": "string",
                  "enum": ["and", "or"]
                }
              },
              "required": ["resolutionStrategy"]
            }
          }
        ]
      }
    },
