{
  "name": "GOOGLE_ADWORDS_REMARKETING_LISTS",
  "displayName": "Google Ads Remarketing Lists (Customer Match)",
  "config": {
    "auth": {
      "type": "OAuth",
      "provider": "Google",
      "role": "google_adwords_remarketing_lists_v1",
      "rudderScopes": ["delivery"]
    },
    "transformAt": "router",
    "transformAtV1": "router",
    "saveDestinationResponse": true,
    "includeKeys": ["oneTrustCookieCategories"],
    "excludeKeys": [],
    "supportedSourceTypes": ["cloud", "warehouse", "shopify"],
    "syncBehaviours": ["mirror"],
    "disableJsonMapper": true,
    "supportsVisualMapper": true,
    "supportedMessageTypes": ["audiencelist"],
    "isAudienceSupported": true,
    "supportsBlankAudienceCreation": true,
    "destConfig": {
      "defaultConfig": [
        "rudderAccountId",
        "listId",
        "customerId",
        "loginCustomerId",
        "subAccount",
        "userSchema",
        "isHashRequired",
        "typeOfList",
        "oneTrustCookieCategories"
      ],
      "cloud": [
        "audienceId"
      ]
    },
    "secretKeys": []
  },
  "options": { "isBeta": false }
}
