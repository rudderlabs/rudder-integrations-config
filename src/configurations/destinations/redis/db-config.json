      "web": ["consentManagement", "connectionMode", "shouldMergeJSON"],
      "unity": ["consentManagement", "connectionMode", "shouldMergeJSON"],
      "amp": ["consentManagement", "connectionMode", "shouldMergeJSON"],
      "cloud": ["consentManagement", "connectionMode", "shouldMergeJSON"],
      "warehouse": ["consentManagement", "connectionMode"],
      "reactnative": ["consentManagement", "connectionMode", "shouldMergeJSON"],
      "flutter": ["consentManagement", "connectionMode", "shouldMergeJSON"],
      "cordova": ["consentManagement", "connectionMode", "shouldMergeJSON"],
      "shopify": ["consentManagement", "connectionMode", "shouldMergeJSON"]
    }
  }
}
