    "includeKeys": ["oneTrustCookieCategories", "ketchConsentPurposes", "consentManagement"],
    "excludeKeys": [],
    "secretKeys": ["password", "caCertificate"],
    "supportedSourceTypes": [
      "android",
      "ios",
      "web",
      "unity",
      "amp",
      "cloud",
      "warehouse",
      "reactnative",
      "flutter",
      "cordova",
      "shopify"
    ],
    "supportedMessageTypes": { "cloud": ["identify"] },
    "supportedConnectionModes": {
      "android": ["cloud"],
      "ios": ["cloud"],
      "web": ["cloud"],
      "unity": ["cloud"],
      "amp": ["cloud"],
      "reactnative": ["cloud"],
      "flutter": ["cloud"],
      "cordova": ["cloud"],
      "shopify": ["cloud"],
      "cloud": ["cloud"],
      "warehouse": ["cloud"]
    },
    "destConfig": {
      "defaultConfig": [
        "address",
        "database",
        "password",
        "prefix",
        "secure",
