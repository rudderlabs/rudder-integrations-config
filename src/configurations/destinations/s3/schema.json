{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["bucketName"],
    "properties": {
      "bucketName": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "prefix": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "iamRoleARN": {
        "type": "string"
      },
      "roleBasedAuth": {
        "type": "boolean"
      },
      "accessKeyID": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "enableSSE": {
        "type": "boolean"
      }
    }
  }
}
