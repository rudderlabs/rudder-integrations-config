{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "required": ["apiKey", "audienceId", "datacenterId"],
    "type": "object",
    "properties": {
      "apiKey": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "audienceId": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "datacenterId": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "enableMergeFields": {
        "type": "boolean",
        "default": false
      },
      "oneTrustCookieCategories": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "oneTrustCookieCategory": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      }
    }
  }
}
