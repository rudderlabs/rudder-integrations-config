{
  "name": "CRITEO_AUDIENCE",
  "displayName": "Criteo Audience",
  "config": {
    "supportsBlankAudienceCreation": true,
    "disableJsonMapper": true,
    "isAudienceSupported": true,
    "supportsVisualMapper": true,
    "auth": {
      "type": "OAuth"
    },
    "transformAtV1": "router",
    "saveDestinationResponse": true,
    "includeKeys": ["oneTrustCookieCategories", "ketchConsentPurposes", "consentManagement"],
    "excludeKeys": [],
    "supportedSourceTypes": ["cloud", "warehouse", "shopify"],
    "syncBehaviours": ["mirror"],
    "supportedMessageTypes": {
      "cloud": ["audiencelist"]
    },
    "supportedConnectionModes": {
      "shopify": ["cloud"],
      "cloud": ["cloud"],
      "warehouse": ["cloud"]
    },
    "destConfig": {
      "defaultConfig": [
        "rudderAccountId",
        "gumCallerId",
        "audienceType",
        "audienceId",
        "adAccountId"
      ],
      "cloud": [
        "connectionMode",
        "consentManagement",
        "oneTrustCookieCategories",
        "ketchConsentPurposes"
      ],
      "warehouse": [
        "adAccountId",
        "connectionMode",
        "consentManagement",
        "oneTrustCookieCategories",
        "ketchConsentPurposes"
      ],
      "shopify": [
        "connectionMode",
        "consentManagement",
        "oneTrustCookieCategories",
        "ketchConsentPurposes"
      ]
    }
  }
}
