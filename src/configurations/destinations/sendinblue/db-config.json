      "defaultConfig": [
        "apiKey",
        "clientKey",
        "doi",
        "templateId",
        "redirectionUrl",
        "contactAttributeMapping",
        "sendTraitsInTrack",
        "whitelistedEvents",
        "blacklistedEvents",
