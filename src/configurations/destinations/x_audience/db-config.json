{
  "name": "X_AUDIENCE",
  "displayName": "X Audience (formerly Twitter Audience)",
  "config": {
    "auth": { "type": "OAuth", "role": "x_audience", "rudderScopes": ["delivery"] },
    "transformAtV1": "router",
    "excludeKeys": [],
    "supportedSourceTypes": ["cloud", "warehouse"],
    "isAudienceSupported": true,
    "supportedConnectionModes": {
      "cloud": ["cloud"],
      "warehouse": ["cloud"]
    },
    "supportsBlankAudienceCreation": true,
    "syncBehaviours": ["mirror"],
    "disableJsonMapper": true,
    "supportsVisualMapper": true,
    "destConfig": {
      "defaultConfig": ["rudderAccountId", "audienceId", "accountId", "enableHash"],
      "cloud": ["connectionMode", "consentManagement"],
      "warehouse": ["connectionMode", "consentManagement"]
    },
    "secretKeys": [],
    "supportedMessageTypes": { "cloud": ["record"] }
  },
  "options": { "isBeta": true }
}
