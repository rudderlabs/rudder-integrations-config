{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["accountId"],
    "properties": {
      "accountId": {
        "type": "string",
        "pattern": "(^env[.].+)|^(.{1,50})$"
      },
      "enableHash": {
        "type": "boolean",
        "default": false
      },
      "connectionMode": {
        "type": "object",
        "properties": {
          "cloud": {
            "type": "string",
            "enum": ["cloud"]
          },
          "warehouse": {
            "type": "string",
            "enum": ["cloud"]
          }
        }
      },
      "oneTrustCookieCategories": {
        "type": "object",
        "properties": {
          "cloud": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "oneTrustCookieCategory": {
                  "type": "string",
                  "pattern": "(^env[.].+)|^(.{0,100})$"
                }
              }
            }
          },
          "warehouse": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "oneTrustCookieCategory": {
                  "type": "string",
                  "pattern": "(^env[.].+)|^(.{0,100})$"
                }
              }
            }
          }
        }
      },
      "ketchConsentPurposes": {
        "type": "object",
        "properties": {
          "cloud": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "purpose": {
                  "type": "string",
                  "pattern": "(^env[.].+)|^(.{0,100})$"
                }
              }
            }
          },
          "warehouse": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "purpose": {
                  "type": "string",
                  "pattern": "(^env[.].+)|^(.{0,100})$"
                }
              }
            }
          }
        }
      }
    }
  }
}
