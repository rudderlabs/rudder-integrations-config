  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": [
      "host",
      "database",
      "user",
      "password",
      "port",
      "sslMode",
      "syncFrequency",
      "useRudderStorage"
    ],
    "properties": {
      "host": {
        "type": "string",
        "pattern": "(^env[.].+)|(?!.*\\.ngrok\\.io)^(.{1,100})$"
      },
      "database": {
        "type": "string",
        "pattern": "(^env[.].+)|^(.{1,100})$"
      },
      "user": {
        "type": "string",
        "pattern": "(^env[.].+)|^(.{1,100})$"
      },
      "password": {
        "type": "string",
        "pattern": "(^env[.].+)|.+"
      },
      "port": {
        "type": "string",
        "pattern": "(^env[.].+)|^(.{1,100})$"
      },
      "namespace": {
        "type": "string",
        "pattern": "(^env[.].*)|^((?!pg_|PG_|pG_|Pg_).{0,64})$"
      },
      "sslMode": {
        "type": "string",
        "pattern": "^(disable|true|false)$"
      },
      "syncFrequency": {
        "type": "string",
        "pattern": "^(5|15|30|60|180|360|720|1440)$",
        "default": "180"
      },
      "syncStartAt": {
        "type": "string"
      },
      "excludeWindow": {
        "type": "object",
        "required": ["excludeWindowStartTime", "excludeWindowEndTime"],
        "properties": {
          "excludeWindowStartTime": {
            "type": "string"
          },
          "excludeWindowEndTime": {
            "type": "string"
          }
        }
      },
      "useRudderStorage": {
        "type": "boolean",
        "default": false
      },
      "preferAppend": {
        "type": "boolean",
        "default": true
      },
      "skipTracksTable": {
        "type": "boolean",
        "default": false
      },
      "skipUsersTable": {
        "type": "boolean",
        "default": true
      },
      "bucketProvider": {
        "type": "string",
        "pattern": "^(S3|GCS|AZURE_BLOB|MINIO)$"
