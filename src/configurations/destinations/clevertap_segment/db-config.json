{
  "name": "CLEVERTAP_SEGMENT",
  "displayName": "Clevertap Segment",
  "config": {
    "transformAtV1": "none",
    "saveDestinationResponse": true,
    "isAudienceSupported": true,
    "supportedSourceTypes": ["warehouse"],
    "supportedMessageTypes": {
      "cloud": ["record"]
    },
    "supportedConnectionModes": {
      "warehouse": ["cloud"]
    },
    "destConfig": {
      "defaultConfig": ["appKey", "accessToken", "region"],
      "warehouse": [
        "connectionMode",
        "oneTrustCookieCategories",
        "ketchConsentPurposes",
        "consentManagement"
      ]
    },
    "secretKeys": ["appKey", "accessToken"]
  },
  "options": {
    "isBeta": true
  }
}
