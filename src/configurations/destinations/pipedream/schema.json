{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "required": ["pipedreamUrl"],
    "type": "object",
    "properties": {
      "pipedreamUrl": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(https?|ftp)://[^\\s/$.?#].[^\\s]*$"
      },
      "pipedreamMethod": {
        "type": "string",
        "enum": ["POST", "PUT", "PATCH", "GET", "DELETE"],
        "default": "POST"
      },
      "headers": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "from": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,1000})$"
            },
            "to": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,1000})$"
            }
          }
        }
      },
      "oneTrustCookieCategories": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "oneTrustCookieCategory": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      }
    }
  }
}
