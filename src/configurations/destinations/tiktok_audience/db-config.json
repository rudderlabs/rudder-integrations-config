{
  "name": "TIKTOK_AUDIENCE",
  "displayName": "Tiktok Audience",
  "config": {
    "cdkV2Enabled": true,
    "disableJsonMapper": true,
    "excludeKeys": [],
    "auth": { "type": "OAuth" },
    "includeKeys": ["oneTrustCookieCategories", "consentManagement"],
    "isAudienceSupported": true,
    "saveDestinationResponse": true,
    "supportsBlankAudienceCreation": true,
    "supportedMessageTypes": { "cloud": ["audiencelist"] },
    "supportedSourceTypes": ["cloud", "warehouse"],
    "supportsVisualMapper": true,
    "syncBehaviours": ["mirror"],
    "transformAtV1": "router",
    "destConfig": {
      "defaultConfig": ["rudderAccountId", "isHashRequired", "oneTrustCookieCategories"],
      "cloud": ["audienceId", "connectionMode", "consentManagement"],
      "warehouse": ["adAccountId", "connectionMode", "consentManagement"]
    },
    "supportedConnectionModes": { "cloud": ["cloud"], "warehouse": ["cloud"] }
  },
  "options": { "isBeta": true }
}
