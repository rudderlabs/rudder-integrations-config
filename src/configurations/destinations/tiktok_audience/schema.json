{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "required": ["audienceId"],
    "type": "object",
    "properties": {
      "audienceId": {
        "type": "object",
        "properties": {
          "cloud": {
            "type": "string"
          }
        }
      },
      "adAccountId": {
        "type": "object",
        "properties": {
          "warehouse": {
            "type": "string"
          }
        }
      },
      "isHashRequired": {
        "type": "boolean",
        "default": true
      },
      "oneTrustCookieCategories": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "oneTrustCookieCategory": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "useNativeSDK": {
        "type": "boolean"
      },
      "connectionMode": {
        "type": "object",
        "properties": {}
      }
    }
  }
}
