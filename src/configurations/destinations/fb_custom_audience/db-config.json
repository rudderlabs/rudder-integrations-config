{
  "name": "FB_CUSTOM_AUDIENCE",
  "displayName": "Facebook Custom Audience",
  "config": {
    "disableJsonMapper": true,
    "supportsVisualMapper": true,
    "syncBehaviours": [
      "mirror"
    ],
    "transformAt": "processor",
    "transformAtV1": "processor",
    "saveDestinationResponse": true,
    "includeKeys": ["oneTrustCookieCategories"],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "cloud",
      "warehouse"
    ],
    "supportedMessageTypes": [
      "audiencelist"
    ],
    "isAudienceSupported": true,
    "destConfig": {
      "defaultConfig": [
        "accessToken",
        "maxUserCount",
        "userSchema",
        "isHashRequired",
        "isRaw",
        "disableFormat",
        "skipVerify",
        "type",
        "subType",
        "oneTrustCookieCategories"
      ],
      "cloud": [
        "audienceId"
      ],
      "warehouse": [
        "adAccountId"
      ]
    },
    "secretKeys": [
      "accessToken"
    ]
  },
  "responseRules": {
    "responseType": "JSON",
    "rules": {
      "abortable": [
        { "errors.0.code": 200 },
        { "errors.0.code": 100 },
        { "errors.0.code": 2650 },
        { "errors.0.code": 368 },
        { "errors.0.code": 105 },
        { "errors.0.code": 294 },
        { "errors.0.code": 190 }
      ]
    }
  }
}