{
  "uiConfig": {
    "baseTemplate": [
      {
        "title": "Initial setup",
        "note": "Review how this destination is set up",
        "sections": [
          {
            "groups": [
              {
                "title": "Connection settings",
                "note": "Update your connection settings here",
                "icon": "settings",
                "fields": [
                  {
                    "type": "textInput",
                    "label": "Host",
                    "configKey": "host",
                    "regex": "(?!.*\\.ngrok\\.io)^(.{0,100})$",
                    "regexErrorMessage": "Invalid Host",
                    "placeholder": "e.g: psql.mydomain.com"
                  },
                  {
                    "type": "textInput",
                    "label": "Database",
                    "configKey": "database",
                    "regex": "^(.{0,100})$",
                    "regexErrorMessage": "Invalid Database",
                    "placeholder": "e.g: rudderdb"
                  },
                  {
                    "type": "textInput",
                    "label": "User",
                    "configKey": "user",
                    "regex": "^(.{0,100})$",
                    "regexErrorMessage": "Invalid User",
                    "placeholder": "e.g: rudder"
                  },
                  {
                    "type": "textInput",
                    "label": "Password",
                    "configKey": "password",
                    "placeholder": "e.g: rudder-password",
                    "regex": ".*"
                  },
                  {
                    "type": "textInput",
                    "label": "Port",
                    "configKey": "port",
                    "regex": "^(.{0,100})$",
                    "regexErrorMessage": "Invalid Port",
                    "placeholder": "5432"
                  },
                  {
                    "type": "textInput",
                    "label": "Namespace",
                    "labelNote": "Schema name for the warehouse where the tables are created",
                    "configKey": "namespace",
                    "regex": "^((?!pg_|PG_|pG_|Pg_).{0,64})$",
                    "regexErrorMessage": "Invalid Namespace",
                    "placeholder": "e.g: rudder-schema",
                    "note": "Default will be the source name"
                  },
                  {
                    "type": "singleSelect",
                    "label": "SSL Mode",
                    "configKey": "sslMode",
                    "options": [
                      {
                        "name": "disable",
                        "value": "disable"
                      },
                      {
                        "name": "require",
                        "value": "require"
                      },
                      {
                        "name": "verify ca",
                        "value": "verify-ca"
                      }
                    ],
                    "default": "disable"
                  },
                  {
                    "type": "textInput",
                    "regex": "-----BEGIN RSA PRIVATE KEY-----.*-----END RSA PRIVATE KEY-----",
                    "regexErrorMessage": "Invalid Client Key Pem File",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "sslMode",
                          "value": "verify-ca"
                        }
                      ]
                    },
                    "label": "Client Key Pem File",
                    "configKey": "clientKey"
                  },
                  {
                    "type": "textInput",
                    "regex": "-----BEGIN CERTIFICATE-----.*-----END CERTIFICATE-----",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "sslMode",
                          "value": "verify-ca"
                        }
                      ]
                    },
                    "label": "Client Cert Pem File",
                    "configKey": "clientCert"
                  },
                  {
                    "type": "textInput",
                    "regex": "-----BEGIN CERTIFICATE-----.*-----END CERTIFICATE-----",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "sslMode",
                          "value": "verify-ca"
                        }
                      ]
                    },
                    "label": "Server CA Pem File",
                    "configKey": "serverCA"
                  }
                ]
              }
            ]
          },
          {
            "groups": [
              {
                "title": "Sync settings",
                "note": "Update your sync settings here",
                "icon": "settings",
                "fields": [
                  {
                    "type": "singleSelect",
                    "label": "Sync Frequency",
                    "configKey": "syncFrequency",
                    "options": [
                      {
                        "name": "Every 30 minutes",
                        "value": "30"
                      },
                      {
                        "name": "Every 1 hour",
                        "value": "60"
                      },
                      {
                        "name": "Every 3 hours",
                        "value": "180"
                      },
                      {
                        "name": "Every 6 hours",
                        "value": "360"
                      },
                      {
                        "name": "Every 12 hours",
                        "configKey": "720"
                      },
                      {
                        "name": "Every 24 hours",
                        "configKey": "1440"
                      }
                    ],
                    "default": "30"
                  },
                  {
                    "type": "timePicker",
                    "label": "Sync Starting At (Optional)",
                    "configKey": "syncStartAt",
                    "options": {
                      "omitSeconds": true,
                      "minuteStep": 15
                    },
                    "note": "Note: Please specify time in UTC"
                  },
                  {
                    "type": "timeRangePicker",
                    "label": "Exclude window (Optional)",
                    "configKey": "excludeWindow",
                    "startTime": {
                      "label": "start time",
                      "configKey": "excludeWindowStartTime"
                    },
                    "endTime": {
                      "label": "end time",
                      "configKey": "excludeWindowEndTime"
                    },
                    "options": {
                      "omitSeconds": true,
                      "minuteStep": 1
                    },
                    "note": "Note: Please specify time in UTC"
                  },
                  {
                    "type": "textInput",
                    "label": "JSON columns (Optional)",
                    "labelNote": "Specify required JSON properties in dot notation separated by commas",
                    "configKey": "jsonPaths",
                    "regex": "^(.*)$",
                    "placeholder": "e.g: testMap.nestedMap, testArray",
                    "note": [
                      {
                        "text": "Instructions",
                        "link": "https://www.rudderstack.com/docs/data-warehouse-integrations/postgresql/#configuring-postgresql-destination-in-rudderstack"
                      },
                      " for setting up the JSON columns"
                    ]
                  }
                ]
              }
            ]
          },
          {
            "groups": [
              {
                "title": "Object Storage Configuration",
                "note": "Manage the Object Storage configuration for your destination",
                "icon": "settings",
                "fields": [
                  {
                    "type": "checkbox",
                    "label": "Use RudderStack managed object storage",
                    "configKey": "useRudderStorage",
                    "default": false,
                    "note": "Note: Only available for RudderStack managed data planes"
                  },
                  {
                    "type": "singleSelect",
                    "label": "Choose your Storage Provider",
                    "configKey": "bucketProvider",
                    "options": [
                      {
                        "name": "S3",
                        "value": "S3"
                      },
                      {
                        "name": "GCS",
                        "value": "GCS"
                      },
                      {
                        "name": "AZURE_BLOB",
                        "value": "AZURE_BLOB"
                      },
                      {
                        "name": "MINIO",
                        "value": "MINIO"
                      }
                    ],
                    "default": "MINIO",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    }
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "S3"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    },
                    "label": "Staging S3 Storage Bucket Name",
                    "labelNote": "S3 Bucket to store data before loading into Postgres",
                    "configKey": "bucketName",
                    "regex": "^((?!^xn--)(?!.*\\.\\..*)(?!^(\\d+(\\.|$)){4}$)[a-z0-9][a-z0-9-.]{1,61}[a-z0-9])$",
                    "regexErrorMessage": "Invalid Staging S3 Storage Bucket Name",
                    "placeholder": "e.g: s3-event-logs",
                    "note": "Please make sure the bucket exists in your S3"
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "GCS"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    },
                    "label": "Staging GCS Object Storage Bucket Name",
                    "labelNote": "GCS Bucket to store data before loading into Postgres",
                    "configKey": "bucketName",
                    "regex": "^((?!goog)(?!.*google.*)(?!^(\\d+(\\.|$)){4}$)(?!.*\\.\\..*)[a-z0-9][a-z0-9-._]{1,61}[a-z0-9])$",
                    "regexErrorMessage": "Invalid Staging GCS Object Storage Bucket Name",
                    "placeholder": "e.g: gcs-event-logs",
                    "note": "Please make sure the bucket exists in your GCS"
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "AZURE_BLOB"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    },
                    "label": "Staging Azure Blob Storage Container Name",
                    "labelNote": "Container to store data before loading into Postgres",
                    "configKey": "containerName",
                    "regex": "^(?=.{3,63}$)[a-z0-9]+(-[a-z0-9]+)*$",
                    "regexErrorMessage": "Invalid Staging Azure Blob Storage Container Name",
                    "placeholder": "e.g: azure-event-logs",
                    "note": "Please make sure the container exists in your Azure Blob Storage"
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "MINIO"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    },
                    "label": "Staging MINIO Storage Bucket Name",
                    "labelNote": "MINIO Bucket to store data before loading into Postgres",
                    "configKey": "bucketName",
                    "regex": "^((?!^(\\d+(\\.|$)){4}$)[a-z0-9][a-z0-9-.]{1,61}[a-z0-9])$",
                    "regexErrorMessage": "Invalid Staging MINIO Storage Bucket Name",
                    "placeholder": "e.g: minio-event-logs",
                    "note": "Please make sure the bucket exists in your MINIO"
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "MINIO"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    },
                    "label": "MinIO Endpoint",
                    "configKey": "endPoint",
                    "regex": "(?!.*\\.ngrok\\.io)^(.{0,100})$",
                    "regexErrorMessage": "Invalid MinIO Endpoint",
                    "placeholder": "e.g: play.min.io:9000"
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "MINIO"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    },
                    "label": "MINIO Access Key ID",
                    "configKey": "accessKeyID",
                    "regex": "^(.{0,100})$",
                    "regexErrorMessage": "Invalid MinIO Access Key ID",
                    "placeholder": "e.g: access-key-id"
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "MINIO"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    },
                    "label": "MINIO Secret Access Key",
                    "configKey": "secretAccessKey",
                    "regex": "^(.{0,100})$",
                    "regexErrorMessage": "Invalid MinIO Secret Access Key",
                    "placeholder": "e.g: secret-access-key"
                  },
                  {
                    "type": "checkbox",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "MINIO"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    },
                    "label": "Use SSL for connection",
                    "configKey": "useSSL",
                    "default": true
                  },
                  {
                    "type": "checkbox",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "S3"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    },
                    "label": "Role Based Authentication",
                    "configKey": "roleBasedAuth",
                    "default": true
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "S3"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        },
                        {
                          "configKey": "roleBasedAuth",
                          "value": true
                        }
                      ]
                    },
                    "label": "IAM Role ARN",
                    "configKey": "iamRoleARN",
                    "regex": "^(.{0,100})$",
                    "regexErrorMessage": "Invalid Role ARN",
                    "placeholder": "e.g: arn:aws:iam::123456789012:role/S3Access",
                    "footerURL": {
                      "link": "https://www.rudderstack.com/docs/destinations/aws-iam-role-for-rudderstack/",
                      "text": "Instructions for creating IAM Role"
                    }
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "S3"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        },
                        {
                          "configKey": "roleBasedAuth",
                          "value": false
                        }
                      ]
                    },
                    "label": "AWS Access Key ID",
                    "configKey": "accessKeyID",
                    "regex": "^(.{0,100})$",
                    "regexErrorMessage": "Invalid AWS Access Key ID",
                    "placeholder": "e.g: access-key-id"
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "S3"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        },
                        {
                          "configKey": "roleBasedAuth",
                          "value": false
                        }
                      ]
                    },
                    "label": "AWS Secret Access Key",
                    "configKey": "accessKey",
                    "regex": "^(.{0,100})$",
                    "regexErrorMessage": "Invalid AWS Secret Access Key",
                    "placeholder": "e.g: secret-access-key"
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "AZURE_BLOB"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    },
                    "label": "Azure Blob Storage Account Name",
                    "configKey": "accountName",
                    "regex": "^(.{1,100})$",
                    "regexErrorMessage": "Invalid Azure Blob Storage Account Name",
                    "placeholder": "e.g: account-name"
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "AZURE_BLOB"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        },
                        {
                          "configKey": "useSASTokens",
                          "value": false
                        }
                      ]
                    },
                    "label": "Azure Blob Storage Account Key",
                    "configKey": "accountKey",
                    "regex": "^(.{1,100})$",
                    "regexErrorMessage": "Invalid Azure Blob Storage Account Key",
                    "secret": true,
                    "placeholder": "e.g: account-key"
                  },
                  {
                    "type": "textInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "AZURE_BLOB"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        },
                        {
                          "configKey": "useSASTokens",
                          "value": true
                        }
                      ]
                    },
                    "label": "Azure Blob Storage SAS Token",
                    "configKey": "sasToken",
                    "regex": "^(.+)$",
                    "regexErrorMessage": "Invalid Azure Blob Storage SAS Token",
                    "placeholder": "e.g: sas-token",
                    "secret": true
                  },
                  {
                    "type": "checkbox",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "AZURE_BLOB"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    },
                    "label": "Use shared access signature (SAS) Tokens",
                    "configKey": "useSASTokens",
                    "default": false,
                    "note": "Use this to Grant limited access to Azure Storage resources using shared access signatures (SAS)"
                  },
                  {
                    "type": "textareaInput",
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "bucketProvider",
                          "value": "GCS"
                        },
                        {
                          "configKey": "useRudderStorage",
                          "value": false
                        }
                      ]
                    },
                    "label": "Credentials",
                    "labelNote": "GCP Service Account credentials JSON for RudderStack to use in loading data into your Google Cloud Storage",
                    "configKey": "credentials",
                    "regex": ".*",
                    "note": "Create a service account in your GCP Project for RudderStack with roles of 'storage.objectCreator'"
                  }
                ]
              }
            ]
          },
          {
            "groups": [
              {
                "title": "Connection mode",
                "note": [
                  "Update how you want to route events from your source to destination. ",
                  {
                    "text": "Get help deciding",
                    "link": "https://www.rudderstack.com/docs/destinations/rudderstack-connection-modes/"
                  }
                ],
                "icon": "sliders",
                "fields": []
              }
            ]
          }
        ]
      },
      {
        "title": "Configuration settings",
        "note": "Manage the settings for your destination",
        "sections": [
          {
            "title": "Other settings",
            "note": "Configure advanced RudderStack features here",
            "icon": "otherSettings",
            "groups": [
              {
                "title": "Client-side event filtering",
                "note": "Decide what events are allowed (allowlisting) and blocked (denylisting)",
                "preRequisites": {
                  "fields": [
                    {
                      "configKey": "connectionModes.webDevice",
                      "value": true
                    },
                    {
                      "configKey": "connectionModes.mobileDevice",
                      "value": true
                    }
                  ],
                  "condition": "or"
                },
                "fields": [
                  {
                    "type": "singleSelect",
                    "label": "Choose if you want to turn on events filtering:",
                    "configKey": "eventFilteringOption",
                    "note": "You must select either allowlist or denylist to enable events filtering",
                    "options": [
                      {
                        "label": "Disabled",
                        "value": "disable"
                      },
                      {
                        "label": "Filter via allowlist",
                        "value": "whitelistedEvents"
                      },
                      {
                        "label": "Filter via denylist",
                        "value": "blacklistedEvents"
                      }
                    ],
                    "default": "disable"
                  },
                  {
                    "type": "tagInput",
                    "label": "Allowlisted events",
                    "note": "Input separate events by pressing ‘Enter’.\nInput the events you want to allowlist.",
                    "configKey": "whitelistedEvents",
                    "tagKey": "eventName",
                    "placeholder": "e.g: Anonymous page visit",
                    "default": [
                      {
                        "eventName": ""
                      }
                    ],
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "eventFilteringOption",
                          "value": "whitelistedEvents"
                        }
                      ]
                    }
                  },
                  {
                    "type": "tagInput",
                    "label": "Denylisted events",
                    "note": "Input separate events by pressing ‘Enter’.\nInput the events you want to denylist. ",
                    "configKey": "blacklistedEvents",
                    "tagKey": "eventName",
                    "placeholder": "e.g: Anonymous page visit",
                    "default": [
                      {
                        "eventName": ""
                      }
                    ],
                    "preRequisites": {
                      "fields": [
                        {
                          "configKey": "eventFilteringOption",
                          "value": "blacklistedEvents"
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "title": "OneTrust cookie consent settings",
                "note": [
                  "Enter your OneTrust category names if you have them configured. ",
                  {
                    "text": "Learn more ",
                    "link": "https://www.rudderstack.com/docs/sources/event-streams/sdks/rudderstack-javascript-sdk/onetrust-consent-manager/"
                  },
                  "about RudderStack’s OneTrust Consent Manager feature."
                ],
                "fields": [
                  {
                    "type": "tagInput",
                    "label": "Cookie category name",
                    "note": "Input your OneTrust category names by pressing ‘Enter’ after each entry",
                    "configKey": "oneTrustCookieCategories",
                    "tagKey": "oneTrustCookieCategory",
                    "placeholder": "e.g: Credit card visit",
                    "default": [
                      {
                        "oneTrustCookieCategory": ""
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    ],
    "sdkTemplate": {
      "title": "SDK settings",
      "note": "not visible in the ui",
      "fields": []
    }
  }
}
