{
  "name": "BUGSNAG",
  "displayName": "Bugsnag",
  "config": {
    "transformAtV1": "processor",
    "saveDestinationResponse": true,
    "includeKeys": [
      "apiKey",
      "releaseStage",
      "blacklistedEvents",
      "whitelistedEvents",
      "oneTrustCookieCategories",
      "eventFilteringOption",
      "consentManagement"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": ["android", "ios", "web"],
    "supportedMessageTypes": {
      "device": {
        "web": ["identify"],
        "android": ["identify", "track", "screen"],
        "ios": ["identify", "track", "screen"]
      }
    },
    "supportedConnectionModes": {
      "android": ["cloud", "device"],
      "web": ["cloud", "device"],
      "ios": ["cloud", "device"]
    },
    "destConfig": {
      "defaultConfig": [
        "apiKey",
        "blacklistedEvents",
        "whitelistedEvents",
        "eventFilteringOption",
        "oneTrustCookieCategories"
      ],
      "android": ["useNativeSDK", "connectionMode", "consentManagement"],
      "ios": ["useNativeSDK", "connectionMode", "consentManagement"],
      "web": ["useNativeSDK", "releaseStage", "connectionMode", "consentManagement"]
    },
    "secretKeys": ["apiKey"]
  }
}
