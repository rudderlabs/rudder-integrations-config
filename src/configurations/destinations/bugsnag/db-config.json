{
  "name": "BUGSNAG",
  "displayName": "Bugsnag",
  "config": {
    "transformAtV1": "processor",
    "saveDestinationResponse": true,
    "includeKeys": [
      "apiKey",
      "releaseStage",
      "blacklistedEvents",
      "whitelistedEvents",
      "oneTrustCookieCategories",
      "eventFilteringOption",
      "consentManagement",
      "ketchConsentPurposes"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": ["android", "androidKotlin", "ios", "iosSwift", "web"],
    "supportedMessageTypes": {
      "device": {
        "web": ["identify"],
        "android": ["identify", "track", "screen"],
        "ios": ["identify", "track", "screen"]
      }
    },
    "supportedConnectionModes": {
      "android": ["cloud", "device"],
      "androidKotlin": ["cloud"],
      "web": ["cloud", "device"],
      "ios": ["cloud", "device"],
      "iosSwift": ["cloud"]
    },
    "destConfig": {
      "defaultConfig": ["apiKey", "blacklistedEvents", "whitelistedEvents", "eventFilteringOption"],
      "android": [
        "useNativeSDK",
        "connectionMode",
        "consentManagement",
        "oneTrustCookieCategories",
        "ketchConsentPurposes"
      ],
      "androidKotlin": ["connectionMode", "consentManagement"],
      "ios": [
        "useNativeSDK",
        "connectionMode",
        "consentManagement",
        "oneTrustCookieCategories",
        "ketchConsentPurposes"
      ],
      "iosSwift": ["connectionMode", "consentManagement"],
      "web": [
        "useNativeSDK",
        "releaseStage",
        "connectionMode",
        "consentManagement",
        "oneTrustCookieCategories",
        "ketchConsentPurposes"
      ]
    },
    "secretKeys": []
  }
}
