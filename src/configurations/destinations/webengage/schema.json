{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "required": ["licenseCode", "dataCenter"],
    "type": "object",
    "properties": {
      "apiKey": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "licenseCode": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "dataCenter": { "type": "string", "enum": ["standard", "ind"], "default": "standard" },
      "oneTrustCookieCategories": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "oneTrustCookieCategory": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "hashEmail": { "type": "object", "properties": { "web": { "type": "boolean" } } },
      "hashPhone": { "type": "object", "properties": { "web": { "type": "boolean" } } },
      "useNativeSDK": { "type": "boolean" },
      "connectionMode": {
        "type": "object",
        "properties": { "web": { "type": "string", "enum": ["cloud", "device"] } }
      }
    }
  }
}
