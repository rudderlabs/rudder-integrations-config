    "supportedConnectionModes": {
      "web": ["device"]
    },
    "supportedMessageTypes": {
      "device": { "web": ["track", "page"] }
    },
    "destConfig": {
      "defaultConfig": [
        "accountId",
        "homePageUrl",
        "hashMethod",
        "fieldMapping",
        "blacklistedEvents",
        "whitelistedEvents",
        "eventFilteringOption",
        "eventsToStandard",
        "oneTrustCookieCategories"
      ],
      "web": ["useNativeSDK"]
    },
    "secretKeys": ["accountId"]
  }
