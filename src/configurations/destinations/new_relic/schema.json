{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["accountId", "insertKey"],
    "properties": {
      "accountId": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "insertKey": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "dataCenter": {
        "type": "string",
        "pattern": "^(us|eu)$"
      },
      "customEventType": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "sendDeviceContext": {
        "type": "boolean"
      },
      "sendUserIdanonymousId": {
        "type": "boolean"
      }
    }
  }
}
