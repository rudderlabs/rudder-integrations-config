{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "required": ["accountId", "insertKey"],
    "type": "object",
    "properties": {
      "accountId": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "insertKey": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "dataCenter": {
        "type": "string",
        "enum": ["us", "eu"],
        "default": "us"
      },
      "customEventType": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|.*"
      },
      "sendDeviceContext": {
        "type": "boolean",
        "default": false
      },
      "sendUserIdanonymousId": {
        "type": "boolean",
        "default": false
      },
      "oneTrustCookieCategories": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "oneTrustCookieCategory": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      }
    }
  }
}
