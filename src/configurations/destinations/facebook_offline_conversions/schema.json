{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "required": ["accessToken", "eventsToStandard", "eventsToIds"],
    "type": "object",
    "properties": {
      "accessToken": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,220})$"
      },
      "eventsToStandard": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "from": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            },
            "to": {
              "type": "string",
              "enum": [
                "ViewContent",
                "Search",
                "AddToCart",
                "AddToWishlist",
                "InitiateCheckout",
                "AddPaymentInfo",
                "Purchase",
                "Lead",
                "CompleteRegistration",
                "Other"
              ]
            }
          }
        }
      },
      "eventsToIds": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "from": {
              "type": "string",
              "enum": [
                "ViewContent",
                "Search",
                "AddToCart",
                "AddToWishlist",
                "InitiateCheckout",
                "AddPaymentInfo",
                "Purchase",
                "Lead",
                "CompleteRegistration",
                "Other"
              ]
            },
            "to": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "categoryToContent": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "from": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            },
            "to": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "valueFieldIdentifier": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "limitedDataUSage": {
        "type": "boolean"
      },
      "isHashRequired": {
        "type": "boolean",
        "default": true
      },
      "oneTrustCookieCategories": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "oneTrustCookieCategory": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      }
    }
  }
}
