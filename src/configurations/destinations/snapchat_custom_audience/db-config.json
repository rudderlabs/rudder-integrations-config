{
  "name": "SNAPCHAT_CUSTOM_AUDIENCE",
  "displayName": "Snapchat Custom Audience",
  "config": {
    "auth": {
      "type": "OAuth",
      "role": "snapchat_custom_audience",
      "rudderScopes": [
        "delivery"
      ]
    },
    "transformAt": "router",
    "transformAtV1": "router",
    "saveDestinationResponse": true,
    "includeKeys": [
      "segmentId",
      "schema",
      "disableHashing",
      "oneTrustCookieCategories"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "cloud",
      "warehouse",
      "shopify"
    ],
    "supportedMessageTypes": [
      "audiencelist"
    ],
    "destConfig": {
      "defaultConfig": [
        "rudderAccountId",
        "segmentId",
        "schema",
        "disableHashing",
        "oneTrustCookieCategories"
      ]
    },
    "secretKeys": [
      "segmentId"
    ]
  }
}