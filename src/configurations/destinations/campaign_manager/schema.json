{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["profileId"],
    "properties": {
      "profileId": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,50})$"
      },
      "limitAdTracking": {
        "type": "boolean"
      },
      "childDirectedTreatment": {
        "type": "boolean"
      },
      "nonPersonalizedAd": {
        "type": "boolean"
      },
      "treatmentForUnderage": {
        "type": "boolean"
      }
    }
  }
}
