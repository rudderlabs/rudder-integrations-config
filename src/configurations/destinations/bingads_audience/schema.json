{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["customerAccountId", "customerId", "audienceId"],
    "properties": {
      "customerAccountId": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^[0-9]+$"
      },
      "customerId": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^[0-9]+$"
      },
      "audienceId": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^[0-9]+$"
      },
      "hashEmail": { "type": "boolean", "default": true },
      "oneTrustCookieCategories": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "oneTrustCookieCategory": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      }
    }
  }
}
