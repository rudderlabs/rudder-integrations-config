{
  "name": "BINGADS_AUDIENCE",
  "displayName": "BingAds Audience",
  "config": {
    "cdkV2Enabled": true,
    "supportsVisualMapper": true,
    "auth": {
      "type": "OAuth"
    },
    "transformAtV1": "processor",
    "saveDestinationResponse": true,
    "includeKeys": ["oneTrustCookieCategories", "ketchConsentPurposes", "consentManagement"],
    "excludeKeys": [],
    "supportedSourceTypes": ["cloud", "warehouse", "shopify"],
    "supportedMessageTypes": {
      "cloud": ["audiencelist"]
    },
    "syncBehaviours": ["mirror"],
    "disableJsonMapper": true,
    "isAudienceSupported": true,
    "supportedConnectionModes": {
      "shopify": ["cloud"]
    },
    "destConfig": {
      "defaultConfig": [
        "rudderAccountId",
        "customerAccountId",
        "customerId",
        "audienceId",
        "hashEmail",
        "oneTrustCookieCategories",
        "ketchConsentPurposes"
      ],
      "cloud": ["consentManagement"],
      "warehouse": ["consentManagement"],
      "shopify": ["consentManagement"]
    }
  },
  "options": {
    "isBeta": true
  }
}
