{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["region", "lambda"],
    "properties": {
      "accessKeyId": {
        "type": "string"
      },
      "accessKey": {
        "type": "string"
      },
      "iamRoleARN": {
        "type": "string"
      },
      "roleBasedAuth": {
        "type": "boolean"
      },
      "region": {
        "type": "string"
      },
      "lambda": {
        "type": "string"
      },
      "enableBatchInput": {
        "type": "boolean"
      },
      "maxBatchSize": {
        "type": "string",
        "pattern": "^$|^[1-9]\\d*$"
      },
      "clientContext": {
        "type": "string"
      }
    }
  }
}
