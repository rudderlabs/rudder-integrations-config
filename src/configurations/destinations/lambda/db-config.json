{
  "name": "LAMBDA",
  "displayName": "AWS Lambda",
  "config": {
    "transformAt": "router",
    "transformAtV1": "router",
    "saveDestinationResponse": true,
    "includeKeys": ["oneTrustCookieCategories"],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "amp",
      "android",
      "cordova",
      "cloud",
      "flutter",
      "ios",
      "reactnative",
      "unity",
      "warehouse",
      "web",
      "shopify"
    ],
    "supportedMessageTypes": ["identify", "page", "screen", "track", "alias", "group"],
    "destConfig": {
      "defaultConfig": [
        "region",
        "iamRoleARN",
        "accessKeyId",
        "accessKey",
        "lambda",
        "enableBatchInput",
        "clientContext",
        "roleBasedAuth",
        "maxBatchSize",
        "oneTrustCookieCategories"
      ]
    },
    "secretKeys": ["accessKeyId", "accessKey"]
  }
}
