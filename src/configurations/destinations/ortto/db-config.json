    "supportedSourceTypes": [
      "android",
      "ios",
      "web",
      "unity",
      "amp",
      "cloud",
      "warehouse",
      "reactnative",
      "flutter",
      "cordova",
      "shopify"
    ],
    "supportedConnectionModes": {
      "android": ["cloud"],
      "ios": ["cloud"],
      "web": ["cloud"],
      "unity": ["cloud"],
      "amp": ["cloud"],
      "reactnative": ["cloud"],
      "flutter": ["cloud"],
      "cordova": ["cloud"],
      "shopify": ["cloud"],
      "cloud": ["cloud"],
      "warehouse": ["cloud"]
