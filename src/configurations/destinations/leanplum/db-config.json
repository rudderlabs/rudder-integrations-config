        "connectionMode",
        "oneTrustCookieCategories"
      ],
      "ios": ["useNativeSDK", "useNativeSDKToSend", "connectionMode", "oneTrustCookieCategories"],
      "flutter": [
        "useNativeSDK",
        "useNativeSDKToSend",
        "connectionMode",
        "oneTrustCookieCategories"
      ],
      "web": ["oneTrustCookieCategories"],
      "unity": ["oneTrustCookieCategories"],
      "reactnative": ["oneTrustCookieCategories"],
      "cordova": ["oneTrustCookieCategories"]
    },
    "secretKeys": ["clientKey"]
  }
