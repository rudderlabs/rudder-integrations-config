    "includeKeys": [
      "applicationId",
      "clientKey",
      "isDevelop",
      "useNativeSDKToSend",
      "blacklistedEvents",
      "whitelistedEvents",
      "eventFilteringOption",
      "oneTrustCookieCategories",
      "connectionMode"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "android",
      "ios",
      "web",
      "unity",
      "amp",
      "cloud",
      "warehouse",
      "reactnative",
      "flutter",
      "cordova",
      "shopify"
    ],
    "supportedConnectionModes": {
      "android": ["cloud", "device", "hybrid"],
      "ios": ["cloud", "device", "hybrid"],
      "flutter": ["cloud", "device", "hybrid"],
      "web": ["cloud"],
      "unity": ["cloud"],
      "amp": ["cloud"],
      "reactnative": ["cloud"],
      "cordova": ["cloud"],
      "shopify": ["cloud"]
