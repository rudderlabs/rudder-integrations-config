{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["applicationId", "clientKey"],
    "properties": {
      "applicationId": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "clientKey": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "isDevelop": {
        "type": "boolean"
      },
      "eventFilteringOption": {
        "type": "string",
        "pattern": "(^env[.].+)|^(disable|whitelistedEvents|blacklistedEvents)$"
      },
      "connectionMode": {
        "type": "object",
        "properties": {
          "flutter": {
            "type": "string",
            "pattern": "^(cloud|device|hybrid)$"
          },
          "ios": {
            "type": "string",
            "pattern": "^(cloud|device|hybrid)$"
          },
          "android": {
            "type": "string",
            "pattern": "^(cloud|device|hybrid)$"
          }
        }
      },
      "whitelistedEvents": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "eventName": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "blacklistedEvents": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "eventName": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "useNativeSDK": {
        "type": "object",
        "properties": {
          "android": {
            "type": "boolean"
          },
          "ios": {
            "type": "boolean"
          },
          "flutter": {
            "type": "boolean"
          }
        }
      },
      "oneTrustCookieCategories": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "oneTrustCookieCategory": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      }
    }
  }
}
