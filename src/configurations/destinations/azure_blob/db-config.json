{
  "name": "AZURE_BLOB",
  "displayName": "Azure Blob Storage",
  "config": {
    "transformAtV1": "none",
    "saveDestinationResponse": true,
    "includeKeys": ["oneTrustCookieCategories"],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "android",
      "ios",
      "web",
      "unity",
      "amp",
      "cloud",
      "warehouse",
      "reactnative",
      "flutter",
      "cordova",
      "shopify"
    ],
    "destConfig": {
      "defaultConfig": [
        "containerName",
        "prefix",
        "accountName",
        "accountKey",
        "useSASTokens",
        "sasToken",
        "oneTrustCookieCategories"
      ]
    },
    "secretKeys": ["accountKey", "sasToken"]
  }
}
