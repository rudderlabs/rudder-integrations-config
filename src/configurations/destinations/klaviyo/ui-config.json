{
    "uiConfig": [
        {
            "title": "Connection Settings",
            "fields": [
                {
                    "type": "textInput",
                    "label": "Public API Key",
                    "value": "publicApiKey",
                    "inputFieldType": "text",
                    "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
                    "required": true,
                    "placeholder": "e.g. WPqij9",
                    "footerNote": "Your Public API Key",
                    "secret": false
                },
                {
                    "type": "textInput",
                    "label": "Private API Key",
                    "value": "privateApiKey",
                    "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
                    "required": true,
                    "placeholder": "e.g. c59e3838",
                    "secret": true,
                    "footerNote": "Your Private API Key"
                },
                {
                    "type": "textInput",
                    "label": "List Id",
                    "value": "listId",
                    "regex": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
                    "required": false,
                    "placeholder": "e.g. c59e3838",
                    "secret": true,
                    "footerNote": "The default list that you'd like to subscribe and add users to when you call .identify()"
                },
                {
                    "type": "checkbox",
                    "label": "Enable this option to make email as primary identifier",
                    "value": "enforceEmailAsPrimary",
                    "default": false,
                    "required": false
                },
                {
                    "type": "singleSelect",
                    "label": "Consent",
                    "value": "consent",
                    "options": [
                        {
                            "name": "E-mail",
                            "value": "email"
                        },
                        {
                            "name": "Web",
                            "value": "web"
                        },
                        {
                            "name": "SMS",
                            "value": "sms"
                        },
                        {
                            "name": "Direct Mail",
                            "value": "directmail"
                        },
                        {
                            "name": "Mobile",
                            "value": "mobile"
                        }
                    ],
                    "defaultOption": {
                        "name": "E-Mail",
                        "value": "email"
                    },
                    "required": false
                },
                {
                    "type": "checkbox",
                    "label": "SMS Consent",
                    "value": "smsConsent",
                    "default": true,
                    "required": false
                },
                {
                    "type": "checkbox",
                    "label": "Send Page As Track",
                    "value": "sendPageAsTrack",
                    "default": false,
                    "required": false
                },
                {
                    "type": "checkbox",
                    "label": "Additional Page info",
                    "value": "additionalPageInfo",
                    "default": false,
                    "required": false
                }
            ]
        },
        {
            "title": "Native SDK",
            "fields": [
                {
                    "type": "checkbox",
                    "label": "Use device-mode to send events",
                    "value": "useNativeSDK",
                    "default": false,
                    "required": false
                }
            ]
        },
        {
            "title": "Client-side Events Filtering",
            "sectionNote": "Applicable only for device-mode integrations. If enabled, it works only with either allowlisted or denylisted events",
            "fields": [
                {
                    "type": "singleSelect",
                    "value": "eventFilteringOption",
                    "required": false,
                    "options": [
                        {
                            "name": "Disable",
                            "value": "disable"
                        },
                        {
                            "name": "Allowlist",
                            "value": "whitelistedEvents"
                        },
                        {
                            "name": "Denylist",
                            "value": "blacklistedEvents"
                        }
                    ],
                    "defaultOption": {
                        "name": "Disable",
                        "value": "disable"
                    }
                },
                {
                    "type": "dynamicCustomForm",
                    "value": "whitelistedEvents",
                    "label": "Allowlist",
                    "customFields": [
                        {
                            "type": "textInput",
                            "value": "eventName",
                            "required": false,
                            "placeholder": "e.g: Anonymous Page Visit"
                        }
                    ],
                    "required": false
                },
                {
                    "type": "dynamicCustomForm",
                    "value": "blacklistedEvents",
                    "label": "Denylist",
                    "customFields": [
                        {
                            "type": "textInput",
                            "value": "eventName",
                            "required": false,
                            "placeholder": "e.g: Credit Card Added"
                        }
                    ],
                    "required": false
                }
            ]
        },
        {
            "title": "Consent Settings",
            "fields": [
                {
                    "type": "dynamicCustomForm",
                    "value": "oneTrustCookieCategories",
                    "label": "OneTrust Cookie Categories",
                    "customFields": [
                        {
                            "type": "textInput",
                            "placeholder": "Marketing",
                            "value": "oneTrustCookieCategory",
                            "label": "Category Name/ID",
                            "required": false
                        }
                    ],
                    "required": false
                }
            ]
        }
    ]
}