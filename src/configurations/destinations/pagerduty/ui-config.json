{
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "PagerDuty Routing Key",
          "value": "routingKey",
          "regex": "^(.{1,100})$",
          "required": true,
          "secret": true,
          "placeholder": "e.g. e95facc04764012XXXfb002540d5d1a6",
          "footerNote": "Enter the Routing Key from PagerDuty Service"
        }
      ]
    },
    {
      "title": "Other Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "dedup_key Field Identifier",
          "value": "dedupKeyFieldIdentifier",
          "required": false,
          "placeholder": "e.g: properties.dedupKey"
        }
      ]
    },
    {
      "title": "Consent Settings",
      "fields": [
        {
          "type": "dynamicCustomForm",
          "value": "oneTrustCookieCategories",
          "label": "OneTrust Cookie Categories",
          "customFields": [
            {
              "type": "textInput",
              "placeholder": "Marketing",
              "value": "oneTrustCookieCategory",
              "label": "Category Name/ID",
              "required": false
            }
          ]
        }
      ]
    }
  ]
}
