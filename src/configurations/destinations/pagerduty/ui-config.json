{
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "PagerDuty Routing Key",
          "value": "routingKey",
          "regex": "^(.{1,100})$",
          "required": true,
          "secret": true,
          "placeholder": "e.g. e95facc04764012d8bfb002540d5d1a6",
          "footerNote": "Enter the Routing Key from PagerDuty Service"
        }
      ]
    },
    {
      "title": "Other Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "dedup_key Field Identifier",
          "value": "dedupKeyFieldIdentifier",
          "required": false,
          "placeholder": "e.g: properties.dedupKey"
        }
      ]
    }
  ]
}
