      "android",
      "ios",
      "web",
      "unity",
      "amp",
      "cloud",
      "reactnative",
      "flutter",
      "cordova",
      "warehouse",
      "shopify"
    ],
    "supportedMessageTypes": ["identify", "track"],
    "destConfig": {
      "defaultConfig": [
        "apiKey",
        "marketingOptin",
        "allowMarketing",
        "allowTransactional",
        "oneTrustCookieCategories"
      ]
    },
    "secretKeys": ["apiKey"]
  }
}
