    "supportedMessageTypes": {
      "cloud": ["identify", "track"]
    },
    "supportedConnectionModes": {
      "android": ["cloud"],
      "ios": ["cloud"],
      "web": ["cloud"],
      "unity": ["cloud"],
      "amp": ["cloud"],
      "reactnative": ["cloud"],
      "flutter": ["cloud"],
      "cordova": ["cloud"],
      "shopify": ["cloud"]
    },
    "destConfig": {
      "defaultConfig": [
        "clientId",
        "clientSecret",
        "subDomain",
        "createOrUpdateContacts",
        "externalKey",
