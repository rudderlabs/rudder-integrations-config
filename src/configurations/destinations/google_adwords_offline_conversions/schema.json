{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["customerId"],
    "properties": {
      "customerId": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "subAccount": {
        "type": "boolean"
      },
      "loginCustomerId": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "eventsToOfflineConversionsTypeMapping": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "from": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            },
            "to": {
              "type": "string",
              "pattern": "^(call|click|store)$"
            }
          }
        }
      },
      "eventsToConversionsNamesMapping": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "from": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            },
            "to": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "customVariables": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "from": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            },
            "to": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "UserIdentifierSource": {
        "type": "string",
        "pattern": "(^env[.].*)|^(none|UNSPECIFIED|UNKNOWN|FIRST_PARTY|THIRD_PARTY)$"
      },
      "conversionEnvironment": {
        "type": "string",
        "pattern": "(^env[.].*)|^(none|UNSPECIFIED|UNKNOWN|APP|WEB)$"
      },
      "defaultUserIdentifier": {
        "type": "string",
        "pattern": "(^env[.].*)|^(email|phone)$"
      },
      "enableServerSideIdentify": {
        "type": "boolean"
      },
      "validateOnly": {
        "type": "boolean"
      },
      "isCustomerAllowed": {
        "type": "boolean"
      }
    }
  }
}
