{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "required": [
      "token"
    ],
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "apiSecret": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "serviceAccountUserName": {
        "type": "string"
      },
      "serviceAccountSecret": {
        "type": "string"
      },
      "projectId": {
        "type": "string"
      },
      "dataResidency": {
        "type": "string",
        "enum": [
          "us",
          "eu"
        ],
        "default": "us"
      },
      "identityMergeApi": {
        "type": "string",
        "enum": [
          "simplified",
          "original"
        ],
        "default": "original"
      },
      "strictMode": {
        "type": "boolean",
        "default": false
      },
      "ignoreDnt": {
        "type": "object",
        "default": false,
        "properties": {
          "web": {
            "type": "boolean"
          }
        }
      },
      "userDeletionApi": {
        "type": "string",
        "enum": [
          "engage",
          "task"
        ],
        "default": "engage"
      },
      "people": {
        "type": "object",
        "default": false,
        "properties": {
          "web": {
            "type": "boolean"
          }
        }
      },
      "setAllTraitsByDefault": {
        "type": "object",
        "default": false,
        "properties": {
          "web": {
            "type": "boolean"
          }
        }
      },
      "superProperties": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "property": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "peopleProperties": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "property": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "eventIncrements": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "property": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "propIncrements": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "property": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "consolidatedPageCalls": {
        "type": "object",
        "default": true,
        "properties": {
          "web": {
            "type": "boolean"
          }
        }
      },
      "trackCategorizedPages": {
        "type": "object",
        "default": false,
        "properties": {
          "web": {
            "type": "boolean"
          }
        }
      },
      "trackNamedPages": {
        "type": "object",
        "default": false,
        "properties": {
          "web": {
            "type": "boolean"
          }
        }
      },
      "sourceName": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "crossSubdomainCookie": {
        "type": "object",
        "default": false,
        "properties": {
          "web": {
            "type": "boolean"
          }
        }
      },
      "persistence": {
        "type": "string",
        "enum": [
          "none",
          "cookie",
          "localStorage"
        ],
        "default": "none"
      },
      "persistenceType": {
        "type": "string",
        "enum": [
          "none",
          "cookie",
          "localStorage"
        ],
        "default": "cookie"
      },
      "persistenceName": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "secureCookie": {
        "type": "object",
        "default": false,
        "properties": {
          "web": {
            "type": "boolean"
          }
        }
      },
      "groupKeySettings": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "groupKey": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "useNativeSDK": {
        "type": "object",
        "properties": {
          "web": {
            "type": "boolean"
          }
        }
      },
      "useNewMapping": {
        "type": "boolean",
        "default": false
      },
      "eventFilteringOption": {
        "type": "string",
        "enum": [
          "disable",
          "whitelistedEvents",
          "blacklistedEvents"
        ],
        "default": "disable"
      },
      "whitelistedEvents": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "eventName": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "blacklistedEvents": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "eventName": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "oneTrustCookieCategories": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "oneTrustCookieCategory": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      }
    },
    "anyOf": [
      {
        "if": {
          "properties": {
            "userDeletionApi": {
              "const": "task"
            }
          },
          "required": [
            "userDeletionApi"
          ]
        },
        "then": {
          "properties": {
            "gdprApiToken": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
            }
          },
          "required": [
            "gdprApiToken"
          ]
        }
      }
    ]
  }
}