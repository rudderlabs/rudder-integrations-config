{
  "name": "BUGSNAG",
  "displayName": "Bugsnag",
  "config": {
    "transformAt": "processor",
    "transformAtV1": "processor",
    "saveDestinationResponse": true,
    "includeKeys": [
      "apiKey",
      "releaseStage",
      "ssl",
      "blackListedEvents",
      "whiteListedEvents"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "android",
      "ios",
      "web"
    ],
    "destConfig": {
      "defaultConfig": [
        "apiKey"
      ],
      "android": [
        "useNativeSDK"
      ],
      "ios": [
        "useNativeSDK"
      ],
      "web": [
        "useNativeSDK",
        "releaseStage",
        "ssl",
        "blackListedEvents",
        "whiteListedEvents"
      ]
    },
    "secretKeys": [
      "apiKey"
    ]
  },
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "BugSnag Api Key",
          "value": "apiKey",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid BugSnag Api Key",
          "required": true,
          "placeholder": "e.g: \u2022\u2022\u2022\u2022\u2022\u2022\u2022\u2022\u2022\u20225c0d"
        }
      ]
    },
    {
      "title": "Native SDK",
      "fields": [
        {
          "type": "defaultCheckbox",
          "label": "Use device-mode to send events",
          "value": "useNativeSDK",
          "default": true
        }
      ]
    },
    {
      "title": "Other Settings",
      "fields": [
        {
          "type": "checkbox",
          "label": "Release Stage. If turned on : Development stage else : Production",
          "value": "releaseStage"
        },
        {
          "type": "checkbox",
          "label": "SSL",
          "value": "ssl",
          "default": true
        }
      ]
    }
  ]
}