{
  "name": "PENDO",
  "displayName": "Pendo",
  "config": {
    "transformAt": "processor",
    "transformAtV1": "processor",
    "saveDestinationResponse": true,
    "includeKeys": [
      "apiKey",
      "blackListedEvents",
      "whiteListedEvents"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "web"
    ],
    "destConfig": {
      "defaultConfig": [
        "apiKey"
      ],
      "web": [
        "useNativeSDK",
        "blackListedEvents",
        "whiteListedEvents"
      ]
    },
    "secretKeys": []
  },
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "API KEY",
          "value": "apiKey",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid API KEY",
          "required": true,
          "placeholder": "e.g: 4c39zac5-9817-4707-646c-133947b50c3f",
          "secret": true
        }
      ]
    },
    {
      "title": "Native SDK",
      "fields": [
        {
          "type": "defaultCheckbox",
          "label": "Use device-mode to send events",
          "value": "useNativeSDK",
          "default": true
        }
      ]
    }
  ]
}