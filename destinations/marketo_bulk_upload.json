        {
          "type": "textInput",
          "label": "Client Secret",
          "value": "clientSecret",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Client Secret",
          "required": true,
          "placeholder": "e.g: oxzXtNhcuw9YGsdhplKUeEMi8765dsfw",
