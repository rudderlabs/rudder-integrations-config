{
  "name": "AWIN",
  "displayName": "Awin",
  "config": {
    "transformAt": "processor",
    "transformAtV1": "processor",
    "saveDestinationResponse": true,
    "includeKeys": [
      "advertiserId",
      "blackListedEvents",
      "whiteListedEvents"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "web"
    ],
    "supportedMessageTypes": [
      "track"
    ],
    "destConfig": {
      "defaultConfig": [
        "advertiserId"
      ],
      "web": [
        "useNativeSDK",
        "blackListedEvents",
        "whiteListedEvents"
      ]
    },
    "secretKeys": []
  },
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "Advertiser ID",
          "value": "advertiserId",
          "inputFieldType": "text",
          "regex": ".*",
          "required": true,
          "placeholder": "e.g. 12345",
          "secret": false
        }
      ]
    },
    {
      "title": "Native SDK",
      "fields": [
        {
          "type": "defaultCheckbox",
          "label": "Use device-mode to send events",
          "value": "useNativeSDK",
          "default": true
        }
      ]
    }
  ]
}