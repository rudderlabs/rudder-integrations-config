{
  "name": "HS",
  "displayName": "HubSpot",
  "config": {
    "supportsVisualMapper": true,
    "transformAt": "router",
    "transformAtV1": "router",
    "saveDestinationResponse": true,
    "includeKeys": [
      "hubID",
      "blackListedEvents",
      "whiteListedEvents"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "android",
      "ios",
      "web",
      "unity",
      "amp",
      "cloud",
      "warehouse",
      "reactnative",
      "flutter",
      "cordova"
    ],
    "supportedMessageTypes": [
      "identify",
      "track"
    ],
    "destConfig": {
      "defaultConfig": [
        "hubID",
        "apiKey"
      ],
      "web": [
        "useNativeSDK",
        "blackListedEvents",
        "whiteListedEvents"
      ]
    },
    "secretKeys": []
  },
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "Hub ID",
          "value": "hubID",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Hub ID",
          "required": true,
          "placeholder": "e.g: 748991"
        },
        {
          "type": "textInput",
          "label": "API Key",
          "value": "apiKey",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid API Key",
          "required": true,
          "placeholder": "e.g: 05afa518-132c-40db-830a-de0f688902f7"
        }
      ]
    },
    {
      "title": "Native SDK",
      "fields": [
        {
          "type": "checkbox",
          "label": "Use device-mode to send events",
          "value": "useNativeSDK",
          "default": false
        }
      ]
    }
  ]
}