      "conversionID",
      "pageLoadConversions",
      "clickEventConversions",
      "defaultPageConversion",
      "sendPageView",
      "conversionLinker",
      "disableAdPersonalization",
      "blackListedEvents",
      "whiteListedEvents"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "web"
    ],
    "destConfig": {
      "defaultConfig": [
        "conversionID",
        "pageLoadConversions",
        "clickEventConversions",
        "defaultPageConversion",
        "sendPageView",
        "conversionLinker",
        "disableAdPersonalization"
      ],
      "web": [
        "useNativeSDK",
        "blackListedEvents",
        "whiteListedEvents"
      ]
    },
    "secretKeys": []
  },
  "configSchema": null,
  "responseRules": null,
  "name": "GOOGLEADS",
  "displayName": "Google Ads",
  "category": null,
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "Conversion ID",
          "value": "conversionID",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Conversion ID",
          "required": true,
          "placeholder": "e.g: dd412940"
        }
      ]
    },
    {
      "title": "Native SDK",
      "fields": [
        {
          "type": "defaultCheckbox",
          "label": "Use device-mode to send events",
          "value": "useNativeSDK",
          "default": true
        }
      ]
    },
    {
      "title": "Configure Page Load Conversion",
      "fields": [
        {
          "type": "dynamicForm",
          "labelLeft": "Conversion Label",
          "labelRight": "Name",
          "keyLeft": "conversionLabel",
          "keyRight": "name",
          "placeholderLeft": "bpg3CMiIjLMBELXBp8wC",
          "placeholderRight": "name",
          "value": "pageLoadConversions"
        },
        {
          "type": "textInput",
          "label": "Default Conversion Label",
          "value": "defaultPageConversion",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Default Conversion Label",
          "required": false,
          "placeholder": "e.g: bpg3CMiIjLMBELXBp8wC"
        }
      ]
    },
    {
      "title": "Click Event Conversion",
      "fields": [
        {
          "type": "dynamicForm",
          "labelLeft": "Conversion Label",
          "labelRight": "Name",
          "keyLeft": "conversionLabel",
          "keyRight": "name",
          "placeholderLeft": "bpg3CMiIjLMBELXBp8wC",
          "placeholderRight": "name",
          "value": "clickEventConversions"
        }
      ]
    },
    {
      "title": "Additional Settings",
      "fields": [
        {
          "type": "checkbox",
          "label": "Send Page View",
          "value": "sendPageView",
          "default": true
        },
        {
          "type": "checkbox",
          "label": "Conversion Linker",
          "value": "conversionLinker",
          "default": true
        },
        {
          "type": "checkbox",
          "label": "Disable Ad Personalization",
          "value": "disableAdPersonalization",
          "default": false
        }
      ]
    }
  ]
}