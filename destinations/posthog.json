{
  "name": "POSTHOG",
  "displayName": "PostHog",
  "config": {
    "includeKeys": [
      "teamApiKey",
      "yourInstance",
      "autocapture",
      "capturePageView",
      "disableSessionRecording",
      "propertyBlackList",
      "xhrHeaders",
      "blackListedEvents",
      "whiteListedEvents"
    ],
    "transformAt": "processor",
    "transformAtV1": "processor",
    "saveDestinationResponse": true,
    "excludeKeys": [],
    "supportedSourceTypes": [
      "android",
      "ios",
      "web",
      "unity",
      "amp",
      "cloud",
      "warehouse",
      "reactnative",
      "flutter",
      "cordova"
    ],
    "supportedMessageTypes": [
      "alias",
      "group",
      "identify",
      "page",
      "screen",
      "track"
    ],
    "destConfig": {
      "defaultConfig": [
        "teamApiKey",
        "yourInstance"
      ],
      "web": [
        "useNativeSDK",
        "autocapture",
        "capturePageView",
        "disableSessionRecording",
        "propertyBlackList",
        "xhrHeaders",
        "blackListedEvents",
        "whiteListedEvents"
      ]
    },
    "secretKeys": []
  },
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "Team API KEY",
          "value": "teamApiKey",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Team API KEY",
          "required": true,
          "placeholder": "e.g: K_HlRyO3IE73-Tz4ch7LNDs0JGdwbknNGolFc3MdoAF",
          "secret": true
        },
        {
          "type": "textInput",
          "label": "Instance URL",
          "value": "yourInstance",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Instance URL",
          "placeholder": "e.g: https://app.posthog.com",
          "secret": true
        }
      ]
    },
    {
      "title": "Native SDK",
      "fields": [
        {
          "type": "checkbox",
          "label": "Use device-mode to send events",
          "value": "useNativeSDK",
          "required": false,
          "default": false
        },
        {
          "type": "checkbox",
          "label": "Enable autocapture with PostHog",
          "value": "autocapture",
          "required": false,
          "default": false
        },
        {
          "type": "checkbox",
          "label": "Allow PostHog to automatically capture pageview events",
          "value": "capturePageView",
          "required": false,
          "default": false
        },
        {
          "type": "checkbox",
          "label": "Disable session recoding",
          "value": "disableSessionRecording",
          "required": false,
          "default": false
        },
        {
          "type": "checkbox",
          "label": "Disable PostHog cookie",
          "value": "disableCookie",
          "required": false,
          "default": false
        },
        {
          "type": "dynamicForm",
          "label": "Additional headers to pass with XHR requests to PostHog API",
          "labelLeft": "Key",
          "labelRight": "Value",
          "keyLeft": "key",
          "keyRight": "value",
          "value": "xhrHeaders"
        },
        {
          "type": "dynamicCustomForm",
          "value": "propertyBlackList",
          "label": "Property black list",
          "customFields": [
            {
              "type": "textInput",
              "label": "Property",
              "value": "property",
              "regex": ".*",
              "required": false
            }
          ]
        }
      ]
    }
  ]
}