          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid API Key"
        },
        {
          "type": "singleSelect",
          "label": "Region",
          "value": "region",
          "options": [
            {
              "name": "US",
              "value": "US"
            },
            {
              "name": "EU",
              "value": "EU"
            },
            {
              "name": "IND",
              "value": "IND"
            }
          ],
          "defaultOption": {
            "name": "US",
            "value": "US"
          },
          "required": true
        },
        {
          "type": "checkbox",
          "label": "Use device-mode to send events",
          "value": "useNativeSDK",
          "default": false
        },
        {
          "type": "checkbox",
          "label": "Debug Mode",
          "value": "debug",
          "default": false
        }
      ]
    }
  ]
