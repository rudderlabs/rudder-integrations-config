    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "API ID",
          "value": "apiId",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid API ID",
          "placeholder": "e.g: W0ZHNMPI204KHJ84ZILZACRB",
          "required": true
        },
        {
          "type": "textInput",
          "label": "API Key",
          "value": "apiKey",
          "placeholder": "e.g: uUIHmnX66ctEDjD__bjlSCOOO",
