{
  "name": "LINKEDIN_INSIGHT_TAG",
  "displayName": "Linkedin Insight Tag",
  "config": {
    "transformAt": "processor",
    "saveDestinationResponse": true,
    "includeKeys": [
      "partnerId",
      "blackListedEvents",
      "whiteListedEvents"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "web"
    ],
    "destConfig": {
      "defaultConfig": [
        "partnerId"
      ],
      "web": [
        "useNativeSDK",
        "blackListedEvents",
        "whiteListedEvents"
      ]
    }
  },
  "secretKeys": [],
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "Partner ID",
          "value": "partnerId",
          "inputFieldType": "text",
          "regex": ".*",
          "required": true,
          "placeholder": "e.g. 12345",
          "footerNote": "Your Partner ID",
          "secret": false
        }
      ]
    },
    {
      "title": "Native SDK",
      "fields": [
        {
          "type": "defaultCheckbox",
          "label": "Use device-mode to send events",
          "value": "useNativeSDK",
          "default": true
        }
      ]
    }
  ]
}