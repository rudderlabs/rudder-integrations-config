{
  "config": {
    "transformAt": "processor",
    "transformAtV1": "processor",
    "saveDestinationResponse": false,
    "includeKeys": [
      "measurementId",
      "blockPageViewEvent",
      "sendUserId",
      "extendPageViewParams",
      "blackListedEvents",
      "whiteListedEvents"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "web"
    ],
    "destConfig": {
      "defaultConfig": [
        "measurementId",
        "blockPageViewEvent",
        "sendUserId",
        "extendPageViewParams"
      ],
      "web": [
        "useNativeSDK",
        "blackListedEvents",
        "whiteListedEvents"
      ]
    },
    "secretKeys": []
  },
  "configSchema": null,
  "responseRules": null,
  "name": "GA4",
  "displayName": "Google Analytics 4",
  "category": null,
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "Measurement Id",
          "value": "measurementId",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Measurement Id",
          "required": true,
          "placeholder": "e.g: G-AB1CD2E34F",
          "secret": true
        }
      ]
    },
    {
      "title": "Other Settings",
      "fields": [
        {
          "type": "checkbox",
          "label": "Block a Page View Event",
          "value": "blockPageViewEvent",
          "default": false
        },
        {
          "type": "checkbox",
          "label": "Extend Page View Property",
          "value": "extendPageViewParams",
          "default": false
        },
        {
          "type": "checkbox",
          "label": "Send userId to GA",
          "value": "sendUserId",
          "default": false
        }
      ]
    },
    {
      "title": "Native SDK",
      "fields": [
        {
          "type": "defaultCheckbox",
          "label": "Use device-mode to send events",
          "value": "useNativeSDK",
          "default": true
        }
      ]
    }
  ]
}