{
  "name": "GTM",
  "displayName": "Google Tag Manager",
  "config": {
    "transformAt": "processor",
    "transformAtV1": "processor",
    "saveDestinationResponse": false,
    "includeKeys": [
      "containerID",
      "blackListedEvents",
      "whiteListedEvents"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "web"
    ],
    "destConfig": {
      "defaultConfig": [
        "containerID"
      ],
      "web": [
        "useNativeSDK",
        "blackListedEvents",
        "whiteListedEvents"
      ]
    },
    "secretKeys": []
  },
  "uiConfig": [
    {
      "title": "1. Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "Container ID",
          "value": "containerID",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Container ID",
          "required": true,
          "placeholder": "e.g: GTM-ABCDEFG"
        }
      ]
    },
    {
      "title": "2. Native SDK",
      "fields": [
        {
          "type": "defaultCheckbox",
          "label": "Use device-mode to send events",
          "value": "useNativeSDK",
          "default": true
        }
      ]
    }
  ]
}