{
  "config": {
    "supportsVisualMapper": true,
    "transformAt": "processor",
    "transformAtV1": "processor",
    "saveDestinationResponse": true,
    "includeKeys": [
      "appKey",
      "dataCenter",
      "blackListedEvents",
      "whiteListedEvents"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "android",
      "ios",
      "web",
      "unity",
      "amp",
      "cloud",
      "warehouse",
      "reactnative",
      "flutter",
      "cordova"
    ],
    "supportedMessageTypes": [
      "group",
      "identify",
      "page",
      "screen",
      "track"
    ],
    "destConfig": {
      "defaultConfig": [
        "appKey",
        "dataCenter",
        "restApiKey"
      ],
      "android": [
        "useNativeSDK"
      ],
      "ios": [
        "useNativeSDK"
      ],
      "reactnative": [
        "useNativeSDK"
      ],
      "web": [
        "useNativeSDK",
        "blackListedEvents",
        "whiteListedEvents"
      ]
    },
    "secretKeys": [
      "restApiKey"
    ]
  },
  "configSchema": null,
  "responseRules": null,
  "name": "BRAZE",
  "displayName": "Braze",
  "category": null,
  "uiConfig": [
    {
      "title": "1. Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "App Key",
          "value": "appKey",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid App Key",
          "required": true,
          "placeholder": "e.g: f2c7c97a-48d1-11ea-8e07-faffc22c7c7f"
        },
        {
          "type": "textInput",
          "label": "Rest Api Key",
          "value": "restApiKey",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Rest Api Key",
          "required": true,
          "placeholder": "e.g: 06c19c59-48d2-11ea-b27a-faffc22c7c7f",
          "secret": true
        },
        {
          "type": "textInput",
          "label": "Data Center",
          "value": "dataCenter",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Data Center",
          "required": true,
          "placeholder": "e.g: EU-01"
        }
      ]
    },
    {
      "title": "2. Custom html settings",
      "fields": [
        {
          "type": "checkbox",
          "label": "Enable html in app messages",
          "value": "enableHtmlInAppMessages",
          "default": false
        },
        {
          "type": "checkbox",
          "label": "Allow User Supplied Javascript",
          "value": "allowUserSuppliedJavascript",
          "default": false
        }
      ]
    },
    {
      "title": "3. Native SDK",
      "fields": [
        {
          "type": "checkbox",
          "label": "Use device-mode to send events",
          "value": "useNativeSDK",
          "default": true
        }
      ]
    }
  ]
}