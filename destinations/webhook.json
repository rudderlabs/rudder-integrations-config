{
  "name": "WEBHOOK",
  "displayName": "Webhook",
  "config": {
    "transformAt": "processor",
    "transformAtV1": "processor",
    "saveDestinationResponse": false,
    "includeKeys": [],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "android",
      "ios",
      "web",
      "unity",
      "amp",
      "cloud",
      "warehouse",
      "reactnative",
      "flutter",
      "cordova"
    ],
    "destConfig": {
      "defaultConfig": [
        "webhookUrl",
        "webhookMethod",
        "headers"
      ]
    },
    "secretKeys": [
      "headers.to"
    ]
  },
  "uiConfig": [
    {
      "title": "Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "Webhook URL",
          "value": "webhookUrl",
          "required": true,
          "placeholder": "http://www.abcd.com",
          "regex": "^(?:http(s)?:\\/\\/)?[\\w.-]+(?:\\.[\\w\\.-]+)+[\\w\\-\\._~:/?#[\\]@!\\$&'\\(\\)\\*\\+,;=.]+$"
        },
        {
          "type": "singleSelect",
          "label": "URL Method",
          "value": "webhookMethod",
          "placeholder": "POST",
          "options": [
            {
              "name": "POST",
              "value": "POST"
            },
            {
              "name": "GET",
              "value": "GET"
            }
          ],
          "defaultOption": {
            "name": "POST",
            "value": "POST"
          }
        },
        {
          "type": "dynamicForm",
          "label": "Headers",
          "labelLeft": "Key",
          "labelRight": "Value",
          "keyLeft": "from",
          "keyRight": "to",
          "placeholderLeft": "content-type",
          "placeholderRight": "application/json",
          "value": "headers"
        }
      ]
    }
  ]
}