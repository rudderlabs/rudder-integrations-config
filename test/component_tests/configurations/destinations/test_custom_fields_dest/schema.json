{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "required": ["apiKey"],
    "type": "object",
    "properties": {
      "apiKey": {
        "type": "string",
        "pattern": "^(.{1,100})$"
      },
      "customField": {
        "type": "string",
        "pattern": "^(.{1,100})$"
      },
      "connectionMode": {
        "type": "object",
        "properties": {
          "web": {
            "type": "string",
            "enum": ["cloud"]
          },
          "android": {
            "type": "string",
            "enum": ["cloud"]
          },
          "ios": {
            "type": "string",
            "enum": ["cloud"]
          }
        }
      }
    },
    "additionalProperties": false
  }
}
