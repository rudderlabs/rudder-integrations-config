{
  "uiConfig": [
    {
      "fields": [
        {
          "label": "Brand Id",
          "placeholder": "TV-54xxxx18-1",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Brand Id",
          "required": true,
          "type": "textInput",
          "value": "brandId"
        }
      ],
      "title": "Brand Id"
    },
    {
      "fields": [
        {
          "label": "Client ID",
          "placeholder": "4659",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Client ID",
          "required": true,
          "type": "textInput",
          "value": "clientId"
        }
      ],
      "title": "Client Id"
    },
    {
      "fields": [
        {
          "customFields": [
            {
              "label": "Event Name",
              "placeholder": "e.g: Order Completed",
              "regex": "^(.{0,100})$",
              "regexErrorMessage": "Invalid Event Name",
              "required": false,
              "type": "textInput",
              "value": "event"
            }
          ],
          "labelNote": "Add the events you want to pass to TVSquared",
          "type": "dynamicCustomForm",
          "value": "eventWhiteList"
        }
      ],
      "title": "Event Whitelist"
    },
    {
      "fields": [
        {
          "customFields": [
            {
              "label": "Property Name",
              "placeholder": "e.g: browser",
              "regex": "^(.{0,100})$",
              "regexErrorMessage": "Invalid Property Name",
              "required": false,
              "type": "textInput",
              "value": "propertyName"
            }
          ],
          "labelNote": "List of Properties you want to pass with the event. Default properties are already sent.",
          "type": "dynamicCustomForm",
          "value": "customMetrics"
        }
      ],
      "title": "Custom Metrics"
    },
    {
      "fields": [
        {
          "default": true,
          "label": "Use device-mode to send events",
          "type": "defaultCheckbox",
          "value": "useNativeSDK"
        }
      ],
      "title": "Native SDK"
    },
    {
      "fields": [
        {
          "defaultOption": {
            "name": "Disable",
            "value": "disable"
          },
          "options": [
            {
              "name": "Disable",
              "value": "disable"
            },
            {
              "name": "Allowlist",
              "value": "whitelistedEvents"
            },
            {
              "name": "Denylist",
              "value": "blacklistedEvents"
            }
          ],
          "required": false,
          "type": "singleSelect",
          "value": "eventFilteringOption"
        },
        {
          "customFields": [
            {
              "placeholder": "e.g: Anonymous Page Visit",
              "required": false,
              "type": "textInput",
              "value": "eventName"
            }
          ],
          "label": "Allowlist",
          "type": "dynamicCustomForm",
          "value": "whitelistedEvents"
        },
        {
          "customFields": [
            {
              "placeholder": "e.g: Credit Card Added",
              "required": false,
              "type": "textInput",
              "value": "eventName"
            }
          ],
          "label": "Denylist",
          "type": "dynamicCustomForm",
          "value": "blacklistedEvents"
        }
      ],
      "sectionNote": "Applicable only for device-mode integrations. If enabled, it works only with either whitelisted or blacklisted events",
      "title": "Client-side Events Filtering"
    },
    {
      "fields": [
        {
          "customFields": [
            {
              "label": "Category Name",
              "placeholder": "Marketing",
              "required": false,
              "type": "textInput",
              "value": "oneTrustCookieCategory"
            }
          ],
          "label": "OneTrust Cookie Categories",
          "type": "dynamicCustomForm",
          "value": "oneTrustCookieCategories"
        }
      ],
      "title": "Consent Settings"
    }
  ]
}