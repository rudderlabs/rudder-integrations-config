{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
      "triggerType"
    ],
    "properties": {
      "apiKeyId": {
        "type": "string"
      },
      "enableBatchInput": {
        "type": "boolean"
      },
      "maxBatchSize": {
        "type": "string",
        "pattern": "^[1-9]\\d*$"
      },
      "triggerType": {
        "type": "string",
        "pattern": "^(Http|Https)$"
      }
    },
    "allOf": [
      {
        "if": {
          "properties": {
            "triggerType": {
              "const": "Http"
            }
          },
          "required": [
            "triggerType"
          ]
        },
        "then": {
          "properties": {
            "googleCloudFunctionUrl": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(?:http(s)?:\\/\\/)?[\\w.-]+(?:\\.[\\w\\.-]+)+[\\w\\-\\._~:/?#[\\]@!\\$&'\\(\\)\\*\\+,;=.]+$"
            }
          },
          "required": [
            "googleCloudFunctionUrl"
          ]
        }
      },
      {
        "if": {
          "properties": {
            "triggerType": {
              "const": "Https"
            }
          },
          "required": [
            "triggerType"
          ]
        },
        "then": {
          "properties": {
            "googleCloudFunctionUrl": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(?:http(s)?:\\/\\/)?[\\w.-]+(?:\\.[\\w\\.-]+)+[\\w\\-\\._~:/?#[\\]@!\\$&'\\(\\)\\*\\+,;=.]+$"
            },
            "gcloudAuthorization": {
              "type": "string",
              "pattern": "(^env[.].+)|.+"
            }
          },
          "required": [
            "googleCloudFunctionUrl",
            "gcloudAuthorization"
          ]
        }
      }
    ]
  }
}