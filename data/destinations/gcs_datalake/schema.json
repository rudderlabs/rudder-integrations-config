{
  "$schema": "http://json-schema.org/draft-07/schema",
  "additionalProperties": true,
  "properties": {
    "bucketName": {
      "pattern": "(^env[.].+)|^(.{1,100})$",
      "type": "string"
    },
    "credentials": {
      "pattern": "(^env[.].+)|.+",
      "type": "string"
    },
    "namespace": {
      "pattern": "(^env[.].*)|^((?!pg_|PG_|pG_|Pg_).{0,64})$",
      "type": "string"
    },
    "prefix": {
      "pattern": "(^env[.].*)|^(.{0,100})$",
      "type": "string"
    },
    "syncFrequency": {
      "default": "30",
      "pattern": "^(30|60|180|360|720|1440)$",
      "type": "string"
    },
    "syncStartAt": {
      "type": "string"
    },
    "tableSuffix": {
      "pattern": "(^env[.].+)|.+",
      "type": "string"
    }
  },
  "required": [
    "bucketName",
    "credentials"
  ],
  "type": "object"
}