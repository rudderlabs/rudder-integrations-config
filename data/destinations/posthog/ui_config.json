{
  "uiConfig": [
    {
      "fields": [
        {
          "label": "Team API KEY",
          "placeholder": "e.g: K_HlRyO3IE73-Tz4ch7LNDs0JGdwbknNGolFc3MdoAF",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Team API KEY",
          "required": true,
          "secret": true,
          "type": "textInput",
          "value": "teamApiKey"
        },
        {
          "label": "Instance URL",
          "placeholder": "e.g: https://app.posthog.com",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Instance URL",
          "secret": true,
          "type": "textInput",
          "value": "yourInstance"
        }
      ],
      "title": "Connection Settings"
    },
    {
      "fields": [
        {
          "default": false,
          "label": "Use device-mode to send events",
          "required": false,
          "type": "checkbox",
          "value": "useNativeSDK"
        },
        {
          "default": false,
          "footerNote": "Enabling this will reduce the amount of data stored in cookies by storing it in local storage instead.",
          "label": "Enable Local Storage for Persistence",
          "required": false,
          "type": "checkbox",
          "value": "enableLocalStoragePersistence"
        },
        {
          "default": false,
          "label": "Enable autocapture with PostHog",
          "required": false,
          "type": "checkbox",
          "value": "autocapture"
        },
        {
          "default": false,
          "label": "Allow PostHog to automatically capture pageview events",
          "required": false,
          "type": "checkbox",
          "value": "capturePageView"
        },
        {
          "default": false,
          "label": "Disable session recoding",
          "required": false,
          "type": "checkbox",
          "value": "disableSessionRecording"
        },
        {
          "default": false,
          "label": "Disable PostHog cookie",
          "required": false,
          "type": "checkbox",
          "value": "disableCookie"
        },
        {
          "keyLeft": "key",
          "keyRight": "value",
          "label": "Additional headers to pass with XHR requests to PostHog API",
          "labelLeft": "Key",
          "labelRight": "Value",
          "type": "dynamicForm",
          "value": "xhrHeaders"
        },
        {
          "customFields": [
            {
              "label": "Property",
              "regex": ".*",
              "required": false,
              "type": "textInput",
              "value": "property"
            }
          ],
          "label": "Property black list",
          "type": "dynamicCustomForm",
          "value": "propertyBlackList"
        }
      ],
      "title": "Native SDK"
    },
    {
      "fields": [
        {
          "defaultOption": {
            "name": "Disable",
            "value": "disable"
          },
          "options": [
            {
              "name": "Disable",
              "value": "disable"
            },
            {
              "name": "Allowlist",
              "value": "whitelistedEvents"
            },
            {
              "name": "Denylist",
              "value": "blacklistedEvents"
            }
          ],
          "required": false,
          "type": "singleSelect",
          "value": "eventFilteringOption"
        },
        {
          "customFields": [
            {
              "placeholder": "e.g: Anonymous Page Visit",
              "required": false,
              "type": "textInput",
              "value": "eventName"
            }
          ],
          "label": "Allowlist",
          "type": "dynamicCustomForm",
          "value": "whitelistedEvents"
        },
        {
          "customFields": [
            {
              "placeholder": "e.g: Credit Card Added",
              "required": false,
              "type": "textInput",
              "value": "eventName"
            }
          ],
          "label": "Denylist",
          "type": "dynamicCustomForm",
          "value": "blacklistedEvents"
        }
      ],
      "sectionNote": "Applicable only for device-mode integrations. If enabled, it works only with either whitelisted or blacklisted events",
      "title": "Client-side Events Filtering"
    },
    {
      "fields": [
        {
          "customFields": [
            {
              "label": "Category Name",
              "placeholder": "Marketing",
              "required": false,
              "type": "textInput",
              "value": "oneTrustCookieCategory"
            }
          ],
          "label": "OneTrust Cookie Categories",
          "type": "dynamicCustomForm",
          "value": "oneTrustCookieCategories"
        }
      ],
      "title": "Consent Settings"
    },
    {
      "fields": [
        {
          "default": false,
          "footerNote": "It is recomended to set this to on as we are deprecating support for the old mapping soon.",
          "label": "Use Updated Mapping for Group calls",
          "type": "checkbox",
          "value": "useV2Group"
        }
      ]
    }
  ]
}