{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
      "apiSecret": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
        "type": "string"
      },
      "blacklistedEvents": {
        "items": {
          "properties": {
            "eventName": {
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
              "type": "string"
            }
          },
          "type": "object"
        },
        "type": "array"
      },
      "blockPageViewEvent": {
        "type": "boolean"
      },
      "enableServerSideIdentify": {
        "type": "boolean"
      },
      "eventFilteringOption": {
        "pattern": "(^env[.].+)|^(disable|whitelistedEvents|blacklistedEvents)$",
        "type": "string"
      },
      "extendPageViewParams": {
        "type": "boolean"
      },
      "firebaseAppId": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
        "type": "string"
      },
      "generateLead": {
        "type": "boolean"
      },
      "generateLeadCurrencyTrait": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
        "type": "string"
      },
      "generateLeadValueTrait": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
        "type": "string"
      },
      "loginSignupMethod": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
        "type": "string"
      },
      "measurementId": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(G-.{1,100})$|^$",
        "type": "string"
      },
      "newOrExistingUserTrait": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
        "type": "string"
      },
      "oneTrustCookieCategories": {
        "properties": {
          "web": {
            "items": {
              "properties": {
                "oneTrustCookieCategory": {
                  "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
                  "type": "string"
                }
              },
              "type": "object"
            },
            "type": "array"
          }
        },
        "type": "object"
      },
      "sendLoginSignup": {
        "type": "boolean"
      },
      "sendUserId": {
        "type": "boolean"
      },
      "typesOfClient": {
        "pattern": "(^env[.].+)|^(gtag|firebase)$",
        "type": "string"
      },
      "useNativeSDK": {
        "properties": {
          "web": {
            "type": "boolean"
          }
        },
        "type": "object"
      },
      "whitelistedEvents": {
        "items": {
          "properties": {
            "eventName": {
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
              "type": "string"
            }
          },
          "type": "object"
        },
        "type": "array"
      }
    },
    "required": [
      "measurementId"
    ],
    "type": "object"
  }
}