{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": false,
    "properties": {
      "apiToken": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
        "type": "string"
      },
      "createUsersAsVerified": {
        "type": "boolean"
      },
      "domain": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
        "type": "string"
      },
      "email": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
        "type": "string"
      },
      "removeUsersFromOrganization": {
        "type": "boolean"
      },
      "sendGroupCallsWithoutUserId": {
        "type": "boolean"
      }
    },
    "required": [
      "email",
      "apiToken",
      "domain"
    ],
    "type": "object"
  }
}