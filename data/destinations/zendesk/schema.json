{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "required": [
      "email",
      "apiToken",
      "domain"
    ],
    "properties": {
      "email": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "apiToken": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "domain": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
      },
      "createUsersAsVerified": {
        "type": "boolean"
      },
      "sendGroupCallsWithoutUserId": {
        "type": "boolean"
      },
      "removeUsersFromOrganization": {
        "type": "boolean"
      }
    }
  }
}