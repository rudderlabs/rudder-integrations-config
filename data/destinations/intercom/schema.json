{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
      "apiKey",
      "appId"
    ],
    "properties": {
      "apiKey": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "appId": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$"
      },
      "mobileApiKeyAndroid": {
        "type": "object",
        "properties": {
          "android": {
            "type": "string",
            "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
          }
        }
      },
      "mobileApiKeyIOS": {
        "type": "object",
        "properties": {
          "ios": {
            "type": "string",
            "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
          }
        }
      },
      "useNativeSDK": {
        "type": "object",
        "properties": {
          "web": {
            "type": "boolean"
          },
          "android": {
            "type": "boolean"
          },
          "ios": {
            "type": "boolean"
          }
        }
      },
      "collectContext": {
        "type": "boolean"
      },
      "sendAnonymousId": {
        "type": "boolean"
      },
      "eventFilteringOption": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(disable|whitelistedEvents|blacklistedEvents)$"
      },
      "whitelistedEvents": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "eventName": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "blacklistedEvents": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "eventName": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "oneTrustCookieCategories": {
        "type": "object",
        "properties": {
          "web": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "oneTrustCookieCategory": {
                  "type": "string",
                  "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
                }
              }
            }
          }
        }
      }
    }
  }
}