{
  "uiConfig": [
    {
      "fields": [
        {
          "label": "Access Token",
          "placeholder": "e.g: FGHrOjU4ZDc0MjEyXzhjYmNfNDZmYl85ODUxX2RjZDk0Mzk1M2VlMDoxOjA=",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Access Token",
          "required": true,
          "type": "textInput",
          "value": "apiKey"
        },
        {
          "label": "App Id",
          "placeholder": "e.g: fll5vd90",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid App Id",
          "required": true,
          "type": "textInput",
          "value": "appId"
        },
        {
          "label": "Android API Key",
          "placeholder": "e.g: android_sdk-67f114561f2267e242466e6687bc7a9ba455cf90",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Android API Key",
          "required": false,
          "type": "textInput",
          "value": "mobileApiKeyAndroid"
        },
        {
          "label": "iOS API Key",
          "placeholder": "e.g: ios_sdk-5fe73e0bb7fcae17a1a75fdbad227191a69f6c00",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid iOS API Key",
          "required": false,
          "type": "textInput",
          "value": "mobileApiKeyIOS"
        }
      ],
      "title": "1. Connection Settings"
    },
    {
      "fields": [
        {
          "default": false,
          "label": "Use device-mode to send events",
          "type": "checkbox",
          "value": "useNativeSDK"
        }
      ],
      "title": "2. Native SDK"
    },
    {
      "fields": [
        {
          "label": "Include Context with Identify Calls",
          "required": false,
          "type": "checkbox",
          "value": "collectContext"
        },
        {
          "default": false,
          "label": "Send AnonymousId as Secondary UserId",
          "type": "checkbox",
          "value": "sendAnonymousId"
        }
      ],
      "title": "3. Other Setttings"
    },
    {
      "fields": [
        {
          "defaultOption": {
            "name": "Disable",
            "value": "disable"
          },
          "options": [
            {
              "name": "Disable",
              "value": "disable"
            },
            {
              "name": "Allowlist",
              "value": "whitelistedEvents"
            },
            {
              "name": "Denylist",
              "value": "blacklistedEvents"
            }
          ],
          "required": false,
          "type": "singleSelect",
          "value": "eventFilteringOption"
        },
        {
          "customFields": [
            {
              "placeholder": "e.g: Anonymous Page Visit",
              "required": false,
              "type": "textInput",
              "value": "eventName"
            }
          ],
          "label": "Allowlist",
          "type": "dynamicCustomForm",
          "value": "whitelistedEvents"
        },
        {
          "customFields": [
            {
              "placeholder": "e.g: Credit Card Added",
              "required": false,
              "type": "textInput",
              "value": "eventName"
            }
          ],
          "label": "Denylist",
          "type": "dynamicCustomForm",
          "value": "blacklistedEvents"
        }
      ],
      "sectionNote": "Applicable only for device-mode integrations. If enabled, it works only with either whitelisted or blacklisted events",
      "title": "Client-side Events Filtering"
    },
    {
      "fields": [
        {
          "customFields": [
            {
              "label": "Category Name",
              "placeholder": "Marketing",
              "required": false,
              "type": "textInput",
              "value": "oneTrustCookieCategory"
            }
          ],
          "label": "OneTrust Cookie Categories",
          "type": "dynamicCustomForm",
          "value": "oneTrustCookieCategories"
        }
      ],
      "title": "Consent Settings"
    }
  ]
}