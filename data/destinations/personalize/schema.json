{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
      "region"
    ],
    "properties": {
      "accessKeyId": {
        "type": "string",
        "pattern": "^(.{0,100})$"
      },
      "secretAccessKey": {
        "type": "string",
        "pattern": "^(.{0,100})$"
      },
      "region": {
        "type": "string",
        "pattern": "^(.{0,100})$"
      },
      "trackingId": {
        "type": "string",
        "pattern": "^(.{0,100})$"
      },
      "datasetARN": {
        "type": "string",
        "pattern": "arn:([a-z/d-]+):personalize:.*:.*:.+"
      },
      "eventChoice": {
        "type": "string",
        "pattern": "(^env[.].*)|^(PutEvents|PutUsers|PutItems)$"
      },
      "customMappings": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "from": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            },
            "to": {
              "type": "string",
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$"
            }
          }
        }
      },
      "disableStringify": {
        "type": "boolean"
      }
    }
  }
}