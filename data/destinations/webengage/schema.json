{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "apiKey": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
      "type": "string"
    },
    "dataCenter": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(standard|ind)$",
      "type": "string"
    },
    "licenseCode": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
      "type": "string"
    }
  },
  "required": [
    "licenseCode",
    "apiKey",
    "dataCenter"
  ],
  "type": "object"
}