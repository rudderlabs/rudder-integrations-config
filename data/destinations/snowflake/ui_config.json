{
  "uiConfig": [
    {
      "fields": [
        {
          "label": "Account",
          "placeholder": "e.g: qya54180.us-east-1",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Account",
          "required": true,
          "type": "textInput",
          "value": "account"
        },
        {
          "label": "Database",
          "placeholder": "e.g: RUDDER_EVENTS",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Database",
          "required": true,
          "type": "textInput",
          "value": "database"
        },
        {
          "label": "Warehouse",
          "placeholder": "e.g: RUDDER_WAREHOUSE",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Database",
          "required": true,
          "type": "textInput",
          "value": "warehouse"
        },
        {
          "label": "User",
          "placeholder": "e.g: RUDDER_USER",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid User",
          "required": true,
          "type": "textInput",
          "value": "user"
        },
        {
          "label": "Password",
          "placeholder": "e.g: RUDDER_PASSWORD",
          "regex": ".*",
          "required": true,
          "secret": true,
          "type": "textInput",
          "value": "password"
        },
        {
          "footerNote": "Default will be the source name",
          "label": "Namespace",
          "labelNote": "Schema name for the warehouse where the tables are created",
          "placeholder": "e.g: RUDDER_NAMESPACE",
          "regex": "^((?!pg_|PG_|pG_|Pg_).{0,64})$",
          "regexErrorMessage": "Invalid Namespace",
          "required": false,
          "type": "textInput",
          "value": "namespace"
        },
        {
          "defaultOption": {
            "name": "Every 30 minutes",
            "value": "30"
          },
          "label": "Sync Frequency",
          "options": [
            {
              "name": "Every 30 minutes",
              "value": "30"
            },
            {
              "name": "Every 1 hour",
              "value": "60"
            },
            {
              "name": "Every 3 hours",
              "value": "180"
            },
            {
              "name": "Every 6 hours",
              "value": "360"
            },
            {
              "name": "Every 12 hours",
              "value": "720"
            },
            {
              "name": "Every 24 hours",
              "value": "1440"
            }
          ],
          "required": false,
          "type": "singleSelect",
          "value": "syncFrequency"
        },
        {
          "footerNote": "Note: Please specify time in UTC",
          "label": "Sync Starting At (Optional)",
          "options": {
            "minuteStep": 15,
            "omitSeconds": true
          },
          "required": false,
          "type": "timePicker",
          "value": "syncStartAt"
        },
        {
          "endTime": {
            "label": "end time",
            "value": "excludeWindowEndTime"
          },
          "footerNote": "Note: Please specify time in UTC",
          "label": "Exclude window (Optional)",
          "options": {
            "minuteStep": 1,
            "omitSeconds": true
          },
          "required": false,
          "startTime": {
            "label": "start time",
            "value": "excludeWindowStartTime"
          },
          "type": "timeRangePicker",
          "value": "excludeWindow"
        }
      ],
      "title": "1. Connection Credentials"
    },
