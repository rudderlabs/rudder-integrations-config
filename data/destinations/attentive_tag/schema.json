{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
      "apiKey": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
        "type": "string"
      },
      "signUpSourceId": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^[0-9]+$",
        "type": "string"
      }
    },
    "required": [
      "apiKey"
    ],
    "type": "object"
  }
}