              "type": "string"
            }
          },
          "type": "object"
        },
        "type": "array"
      },
      "blacklistedEvents": {
        "items": {
          "properties": {
            "eventName": {
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
              "type": "string"
            }
          },
          "type": "object"
        },
        "type": "array"
      },
      "customVersionProperty": {
        "type": "string"
      },
      "debugMode": {
        "type": "boolean"
      },
      "denyUrls": {
        "items": {
          "properties": {
            "denyUrls": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "type": "array"
      },
      "dsn": {
        "type": "string"
      },
      "environment": {
        "type": "string"
      },
      "eventFilteringOption": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(disable|whitelistedEvents|blacklistedEvents)$",
        "type": "string"
      },
      "ignoreErrors": {
        "items": {
          "properties": {
            "ignoreErrors": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "type": "array"
      },
      "includePaths": {
        "items": {
          "properties": {
            "includePaths": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "type": "array"
      },
      "logger": {
        "type": "string"
      },
      "oneTrustCookieCategories": {
        "properties": {
          "web": {
            "items": {
              "properties": {
                "oneTrustCookieCategory": {
                  "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
                  "type": "string"
                }
              },
              "type": "object"
            },
            "type": "array"
          }
        },
        "type": "object"
      },
      "release": {
        "type": "string"
      },
      "serverName": {
        "type": "string"
      },
      "useNativeSDK": {
        "properties": {
          "web": {
            "type": "boolean"
          }
        },
        "type": "object"
      },
      "whitelistedEvents": {
        "items": {
          "properties": {
            "eventName": {
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
