{
  "uiConfig": [
    {
      "fields": [
        {
          "label": "Host",
          "placeholder": "e.g: psql.mydomain.com",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Host",
          "required": true,
          "type": "textInput",
          "value": "host"
        },
        {
          "label": "Database",
          "placeholder": "e.g: rudderdb",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Database",
          "required": true,
          "type": "textInput",
          "value": "database"
        },
        {
          "label": "User",
          "placeholder": "e.g: rudder",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid User",
          "required": true,
          "type": "textInput",
          "value": "user"
        },
        {
          "label": "Password",
          "placeholder": "e.g: rudder-password",
          "regex": ".*",
          "required": true,
          "type": "textInput",
          "value": "password"
        },
        {
          "label": "Port",
          "placeholder": "5432",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Port",
          "required": true,
          "type": "textInput",
          "value": "port"
        },
        {
          "footerNote": "Default will be the source name",
          "label": "Namespace",
          "labelNote": "Schema name for the warehouse where the tables are created",
          "placeholder": "e.g: rudder-schema",
          "regex": "^((?!pg_|PG_|pG_|Pg_).{0,64})$",
          "regexErrorMessage": "Invalid Namespace",
          "required": false,
          "type": "textInput",
          "value": "namespace"
        },
        {
          "defaultOption": {
            "name": "disable",
            "value": "disable"
          },
          "label": "SSL Mode",
          "options": [
            {
              "name": "disable",
              "value": "disable"
            },
            {
              "name": "require",
              "value": "require"
            },
            {
              "name": "verify ca",
              "value": "verify-ca"
            }
          ],
          "required": true,
          "type": "singleSelect",
          "value": "sslMode"
        },
        {
          "label": "Client Key Pem File",
          "preRequisiteField": {
            "name": "sslMode",
            "selectedValue": "verify-ca"
          },
          "required": true,
          "type": "textInput",
          "value": "clientKey"
        },
        {
          "label": "Client Cert Pem File",
          "preRequisiteField": {
            "name": "sslMode",
            "selectedValue": "verify-ca"
          },
          "required": true,
          "type": "textInput",
          "value": "clientCert"
        },
        {
          "label": "Server CA Pem File",
          "preRequisiteField": {
            "name": "sslMode",
            "selectedValue": "verify-ca"
          },
          "required": true,
          "type": "textInput",
          "value": "serverCA"
        },
        {
          "defaultOption": {
            "name": "Every 30 minutes",
            "value": "30"
          },
          "label": "Sync Frequency",
          "options": [
            {
              "name": "Every 30 minutes",
              "value": "30"
            },
            {
              "name": "Every 1 hour",
              "value": "60"
            },
            {
              "name": "Every 3 hours",
              "value": "180"
            },
            {
              "name": "Every 6 hours",
              "value": "360"
            },
            {
              "name": "Every 12 hours",
              "value": "720"
            },
            {
              "name": "Every 24 hours",
              "value": "1440"
            }
          ],
          "required": false,
          "type": "singleSelect",
          "value": "syncFrequency"
        },
        {
          "footerNote": "Note: Please specify time in UTC",
          "label": "Sync Starting At (Optional)",
          "options": {
            "minuteStep": 15,
            "omitSeconds": true
          },
          "required": false,
          "type": "timePicker",
          "value": "syncStartAt"
        },
        {
          "endTime": {
            "label": "end time",
            "value": "excludeWindowEndTime"
          },
          "footerNote": "Note: Please specify time in UTC",
          "label": "Exclude window (Optional)",
          "options": {
            "minuteStep": 1,
            "omitSeconds": true
          },
          "required": false,
          "startTime": {
            "label": "start time",
            "value": "excludeWindowStartTime"
          },
          "type": "timeRangePicker",
          "value": "excludeWindow"
        }
      ],
      "title": "Connection Credentials"
    },
