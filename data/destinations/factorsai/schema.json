{
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "required": [
      "rudderStackApiKey"
    ],
    "type": "object",
    "properties": {
      "rudderStackApiKey": {
        "type": "string",
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,50})$"
      }
    }
  }
}