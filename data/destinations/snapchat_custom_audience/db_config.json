{
  "name": "SNAPCHAT_CUSTOM_AUDIENCE",
  "displayName": "Snapchat Custom Audience",
  "config": {
    "auth": {
      "type": "OAuth"
    },
    "transformAt": "router",
    "transformAtV1": "router",
    "saveDestinationResponse": true,
    "includeKeys": [
      "segmentId",
      "schema",
      "disableHashing"
    ],
    "excludeKeys": [],
    "supportedSourceTypes": [
      "cloud",
      "warehouse"
    ],
    "supportedMessageTypes": [
      "audiencelist"
    ],
    "destConfig": {
      "defaultConfig": [
        "rudderAccountId",
        "segmentId",
        "schema",
        "disableHashing"
      ]
    },
    "secretKeys": [
      "segmentId"
    ]
  }
}