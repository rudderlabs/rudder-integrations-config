{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
      "appKey": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
        "type": "string"
      },
      "blacklistedEvents": {
        "items": {
          "properties": {
            "eventName": {
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,300})$",
              "type": "string"
            }
          },
          "type": "object"
        },
        "type": "array"
      },
      "dataCenter": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
        "type": "string"
      },
      "eventFilteringOption": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(disable|whitelistedEvents|blacklistedEvents)$",
        "type": "string"
      },
      "oneTrustCookieCategories": {
        "properties": {
          "web": {
            "items": {
              "properties": {
                "oneTrustCookieCategory": {
                  "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
                  "type": "string"
                }
              },
              "type": "object"
            },
            "type": "array"
          }
        },
        "type": "object"
      },
      "restApiKey": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
        "type": "string"
      },
      "useNativeSDK": {
        "properties": {
          "android": {
            "type": "boolean"
          },
          "ios": {
            "type": "boolean"
          },
          "reactnative": {
            "type": "boolean"
          },
          "web": {
            "type": "boolean"
          }
        },
        "type": "object"
      },
      "whitelistedEvents": {
        "items": {
          "properties": {
            "eventName": {
              "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
              "type": "string"
            }
          },
          "type": "object"
        },
        "type": "array"
      }
    },
    "required": [
      "appKey",
      "restApiKey",
      "dataCenter"
    ],
    "type": "object"
  }
}