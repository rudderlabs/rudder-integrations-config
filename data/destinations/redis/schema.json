{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "address": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
      "type": "string"
    },
    "caCertificate": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|.*",
      "type": "string"
    },
    "clusterMode": {
      "type": "boolean"
    },
    "database": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
      "type": "string"
    },
    "password": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|.*",
      "type": "string"
    },
    "prefix": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
      "type": "string"
    },
    "secure": {
      "type": "boolean"
    },
    "skipVerify": {
      "type": "boolean"
    }
  },
  "required": [
    "address"
  ],
  "type": "object"
}