{
  "uiConfig": [
    {
      "fields": [
        {
          "label": "Project ID",
          "placeholder": "e.g: 4a1d51eab8e77c0000c521ab",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Project ID",
          "required": true,
          "type": "textInput",
          "value": "projectID"
        },
        {
          "label": "Write Key",
          "regex": "^(.{0,100})$",
          "regexErrorMessage": "Invalid Write Key",
          "required": true,
          "type": "textInput",
          "value": "writeKey"
        }
      ],
      "title": "1. Connection Settings"
    },
    {
      "fields": [
        {
          "default": false,
          "label": "Use device-mode to send events",
          "type": "checkbox",
          "value": "useNativeSDK"
        }
      ],
      "title": "2. Native SDK"
    },
    {
      "fields": [
        {
          "default": false,
          "label": "Geo IP Add On",
          "type": "checkbox",
          "value": "ipAddon"
        },
        {
          "default": false,
          "label": "User Agent Add On",
          "type": "checkbox",
          "value": "uaAddon"
        },
        {
          "default": false,
          "label": "URL Parsing Add On",
          "type": "checkbox",
          "value": "urlAddon"
        },
        {
          "default": false,
          "label": "Referrer Parsing Add On",
          "type": "checkbox",
          "value": "referrerAddon"
        }
      ],
      "title": "3. Add On"
    },
    {
      "fields": [
        {
          "defaultOption": {
            "name": "Disable",
            "value": "disable"
          },
          "options": [
            {
              "name": "Disable",
              "value": "disable"
            },
            {
              "name": "Allowlist",
              "value": "whitelistedEvents"
            },
            {
              "name": "Denylist",
              "value": "blacklistedEvents"
            }
          ],
          "required": false,
          "type": "singleSelect",
          "value": "eventFilteringOption"
        },
        {
          "customFields": [
            {
              "placeholder": "e.g: Anonymous Page Visit",
              "required": false,
              "type": "textInput",
              "value": "eventName"
            }
          ],
          "label": "Allowlist",
          "type": "dynamicCustomForm",
          "value": "whitelistedEvents"
        },
        {
          "customFields": [
            {
              "placeholder": "e.g: Credit Card Added",
              "required": false,
              "type": "textInput",
              "value": "eventName"
            }
          ],
          "label": "Denylist",
          "type": "dynamicCustomForm",
          "value": "blacklistedEvents"
        }
      ],
      "sectionNote": "Applicable only for device-mode integrations. If enabled, it works only with either whitelisted or blacklisted events",
      "title": "Client-side Events Filtering"
    },
    {
      "fields": [
        {
          "customFields": [
            {
              "label": "Category Name",
              "placeholder": "Marketing",
              "required": false,
              "type": "textInput",
              "value": "oneTrustCookieCategory"
            }
          ],
          "label": "OneTrust Cookie Categories",
          "type": "dynamicCustomForm",
          "value": "oneTrustCookieCategories"
        }
      ],
      "title": "Consent Settings"
    }
  ]
}