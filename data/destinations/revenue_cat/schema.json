{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "apiKey": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
      "type": "string"
    },
    "xPlatform": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(ios|android|amazon|macos|stripe|uikitformac)$",
      "type": "string"
    }
  },
  "required": [
    "apiKey"
  ],
  "type": "object"
}