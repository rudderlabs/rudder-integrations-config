{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
      "apiKey": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,500})$",
        "type": "string"
      },
      "appId": {
        "type": "string"
      },
      "pixelId": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
        "type": "string"
      },
      "snapAppId": {
        "type": "string"
      }
    },
    "required": [
      "apiKey"
    ],
    "type": "object"
  }
}