{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "accountId": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
      "type": "string"
    },
    "customEventType": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
      "type": "string"
    },
    "dataCenter": {
      "pattern": "^(us|eu)$",
      "type": "string"
    },
    "insertKey": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
      "type": "string"
    },
    "sendDeviceContext": {
      "type": "boolean"
    },
    "sendUserIdanonymousId": {
      "type": "boolean"
    }
  },
  "required": [
    "accountId",
    "insertKey"
  ],
  "type": "object"
}