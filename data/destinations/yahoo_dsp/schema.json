{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
      "accountId": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^[0-9]+$",
        "type": "string"
      },
      "audienceId": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^[0-9]+$",
        "type": "string"
      },
      "audienceType": {
        "pattern": "^(EMAIL|DEVICE_ID|IP_ADDRESS)$",
        "type": "string"
      },
      "clientId": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
        "type": "string"
      },
      "clientSecret": {
        "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
        "type": "string"
      },
      "hashRequired": {
        "type": "boolean"
      },
      "seedListType": {
        "pattern": "^(GPADVID|IDFA)$",
        "type": "string"
      }
    },
    "required": [
      "clientId",
      "clientSecret",
      "audienceId",
      "accountId"
    ],
    "type": "object"
  }
}