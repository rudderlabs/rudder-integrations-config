{
  "auth": {
    "config": [
      {
        "fields": [
          {
            "label": "Account Name",
            "placeholder": "e.g: account name",
            "regex": ".*",
            "required": true,
            "type": "textInput",
            "value": "rudder_account_name"
          },
          {
            "label": "URL",
            "placeholder": "e.g https://rudderstack.api-us1.com",
            "required": true,
            "type": "textInput",
            "value": "url"
          },
          {
            "label": "API Key",
            "regex": "^(.{0,100})$",
            "regexErrorMessage": "Invalid Value",
            "required": true,
            "type": "textInput",
            "value": "apiKey"
          }
        ]
      }
    ],
    "nameField": "rudder_account_name",
    "secretFields": [
      "apiKey"
    ],
    "type": "form"
  }
}