{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "actid": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
      "type": "string"
    },
    "apiKey": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|.*",
      "type": "string"
    },
    "apiUrl": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(?:http(s)?:\\/\\/)?[\\w.-]+(?:\\.[\\w\\.-]+)+[\\w\\-\\._~:/?#[\\]@!\\$&'\\(\\)\\*\\+,;=.]+$",
      "type": "string"
    },
    "eventKey": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
      "type": "string"
    }
  },
  "required": [
    "apiUrl",
    "apiKey"
  ],
  "type": "object"
}