{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "accessKeyID": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
      "type": "string"
    },
    "bucketName": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{1,100})$",
      "type": "string"
    },
    "enableSSE": {
      "type": "boolean"
    },
    "prefix": {
      "pattern": "(^\\{\\{.*\\|\\|(.*)\\}\\}$)|(^env[.].+)|^(.{0,100})$",
      "type": "string"
    }
  },
  "required": [
    "bucketName"
  ],
  "type": "object"
}