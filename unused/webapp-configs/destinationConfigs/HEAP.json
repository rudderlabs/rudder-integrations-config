[
  {
    "title": "Connection Settings",
    "fields": [
      {
        "type": "textInput",
        "label": "App Id",
        "value": "appId",
        "regex": "^(.{0,100})$",
        "regexErrorMessage": "Invalid App Id",
        "required": true,
        "placeholder": "e.g: 3138654182"
      }
    ]
  },
  {
    "title": "Native SDK",
    "fields": [
      {
        "type": "checkbox",
        "label": "Use device-mode to send events",
        "value": "useNativeSDK",
        "default": false
      }
    ]
  }
]