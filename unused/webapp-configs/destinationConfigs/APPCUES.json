[
  {
    "title": "Connection Settings",
    "fields": [
      {
        "type": "textInput",
        "label": "Account Id",
        "value": "accountId",
        "regex": "^(.{0,100})$",
        "regexErrorMessage": "Invalid Account Id",
        "required": true,
        "placeholder": "e.g: 81429"
      }
    ]
  },
  {
    "title": "Native SDK",
    "fields": [
      {
        "type": "checkbox",
        "label": "Use device-mode to send events",
        "value": "useNativeSDK",
        "default": true
      }
    ]
  }
]
