[
  {
    "title": "1. Connection Settings",
    "fields": [
      {
        "type": "textInput",
        "label": "Site ID",
        "value": "siteID",
        "regex": "^(.*)$",
        "regexErrorMessage": "Invalid Site ID",
        "required": true,
        "placeholder": "e.g: company"
      }
    ]
  },
  {
    "title": "2. Native SDK",
    "fields": [
      {
        "type": "defaultCheckbox",
        "label": "Use native SDK to send events",
        "value": "useNativeSDK",
        "default": true
      }
    ]
  }
]

