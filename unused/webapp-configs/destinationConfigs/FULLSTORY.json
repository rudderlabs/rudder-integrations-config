[
  {
    "title": "Connection Settings",
    "fields": [
      {
        "type": "textInput",
        "label": "FS ORG",
        "value": "fs_org",
        "regex": "^(.{0,100})$",
        "regexErrorMessage": "Invalid FS ORG",
        "required": true,
        "placeholder": "e.g: dd412940"
      },
      {
        "type": "checkbox",
        "label": "FS debug mode",
        "value": "fs_debug_mode",
        "default": true
      }
    ]
  },
  {
    "title": "Native SDK",
    "fields": [
      {
        "type": "defaultCheckbox",
        "label": "Use device-mode to send events",
        "value": "useNativeSDK",
        "default": true
      }
    ]
  }
]