{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "required": [
    "project",
    "bucketName",
    "credentials",
    "syncFrequency"
  ],
  "properties": {
    "project": {
      "type": "string",
      "pattern": "(^env[.].*)|^(.{0,100})$"
    },
    "location": {
      "type": "string",
      "pattern": "(^env[.].*)|^(.{0,100})$"
    },
    "bucketName": {
      "type": "string",
      "pattern": "(^env[.].*)|^(.{0,100})$"
    },
    "prefix": {
      "type": "string",
      "pattern": "(^env[.].*)|^(.{0,100})$"
    },
    "namespace": {
      "type": "string",
      "pattern": "(^env[.].*)|^(.{0,100})$"
    },
    "credentials": {
      "type": "string",
      "pattern": "(^env[.].*)|.*"
    },
    "syncFrequency": {
      "type": "string",
      "pattern": "^(30|60|180|360|720|1440)$",
      "default": "30"
    },
    "syncStartAt": {
      "type": "string"
    },
    "excludeWindow": {
      "type": "object",
      "required": [
        "excludeWindowStartTime",
        "excludeWindowEndTime"
      ],
      "properties": {
        "excludeWindowStartTime": {
          "type": "string"
        },
        "excludeWindowEndTime": {
          "type": "string"
        }
      }
    }
  },
  "additionalProperties": true
}