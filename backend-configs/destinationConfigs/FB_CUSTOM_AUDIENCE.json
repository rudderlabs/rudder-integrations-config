{
  "name": "FB_CUSTOM_AUDIENCE",
  "displayName": "Facebook Custom Audience",
  "config": {
    "supportsVisualMapper":true,
    "syncBehaviours": ["mirror"],
    "transformAt": "processor",
    "transformAtV1": "processor",
    "saveDestinationResponse": true,
    "includeKeys": [],
    "excludeKeys": [],
    "supportedSourceTypes": ["cloud", "warehouse"],
    "supportedMessageTypes": ["track"],
    "destConfig": { "defaultConfig": ["accessToken", "audienceId", "maxUserCount", "userSchema", "isHashRequired", "isRaw", "disableFormat", "type", "subType"] },
    "secretKeys": ["accessToken"]
  },
  "responseRules": {
    "responseType": "JSON",
    "rules": {
      "abortable": [
        { "errors.0.code": 200 },
        { "errors.0.code": 100 },
        { "errors.0.code": 2650 },
        { "errors.0.code": 368 },
        { "errors.0.code": 105 },
        { "errors.0.code": 294 },
        { "errors.0.code": 190 }
      ]
    }
  }
}
