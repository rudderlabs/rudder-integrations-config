[
  {
    "title": "Connection Settings",
    "fields": [
      {
        "type": "textInput",
        "label": "Public API Key",
        "value": "publicApiKey",
        "inputFieldType": "text",
        "regex": ".*",
        "required": true,
        "placeholder": "e.g. WPqij9",
        "footerNote":"Your Public API Key",
        "secret": false
      },
      {
        "type": "textInput",
        "label": "Private API Key",
        "value": "privateApiKey",
        "regex": ".*",
        "required": false,
        "placeholder": "e.g. c59e3838",
        "secret": true,
        "footerNote": "Your Private API Key"
      },
      {
        "type": "textInput",
        "label": "List Id",
        "value": "listId",
        "regex": ".*",
        "required": false,
        "placeholder": "e.g. c59e3838",
        "secret": true,
        "footerNote": "The default list that you'd like to subscribe and add users to when you call .identify()"
      },
      {
        "type": "checkbox",
        "label": "Enable this option to make email as primary identifier",
        "value": "enforceEmailAsPrimary",
        "default": false
      },
      {
        "type": "singleSelect",
        "label": "Consent",
        "value": "consent",
        "options": [
          {
            "name": "E-mail",
            "value": "email"
          },
          {
            "name": "Web",
            "value": "web "
          },
          {
            "name": "SMS",
            "value": "sms"
          },
          {
            "name": "Direct Mail",
            "value": "directmail"
          },
          {
            "name": "Mobile",
            "value": "mobile"
          }
        ],
        "defaultOption": {
          "name": "E-Mail",
          "value": "email"
        },
        "required": false
      },
      {
        "type": "checkbox",
        "label": "SMS Consent",
        "value": "smsConsent",
        "default": true
      },
      {
        "type": "checkbox",
        "label": "Send Page As Track",
        "value": "sendPageAsTrack",
        "default": false
      },
      {
        "type": "checkbox",
        "label": "Additional Page info",
        "value": "additionalPageInfo",
        "default": false
      }
    ]
  },
  {
    "title": "Native SDK",
    "fields": [
      {
        "type": "checkbox",
        "label": "Use device-mode to send events",
        "value": "useNativeSDK",
        "default": false
      }
    ]
  }
]
