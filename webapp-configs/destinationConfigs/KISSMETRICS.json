[
  {
    "title": "Connection Settings",
    "fields": [
      {
        "type": "textInput",
        "label": "API Key",
        "value": "apiKey",
        "regex": "^(.{0,100})$",
        "regexErrorMessage": "Invalid API Key",
        "required": true,
        "placeholder": "e.g: ABCDEFG"
      }
    ]
  },
  {
    "title": "Native SDK",
    "fields": [
      {
        "type": "checkbox",
        "label": "Use device-mode to send events",
        "value": "useNativeSDK",
        "default": false
      }
    ]
  },
  {
    "title": "Prefix Properties",
    "fields": [
      {
        "type": "checkbox",
        "label": "Prefix the properties with page or event name",
        "value": "prefixProperties",
        "default": false
      }
    ]
  }
]