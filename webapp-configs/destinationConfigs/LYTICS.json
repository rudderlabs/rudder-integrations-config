[
  {
    "title": "Connection Settings",
    "fields": [
      {
        "type": "textInput",
        "label": "Data Api Key",
        "placeholder": "e.g: Br3nwOFa770jaUKC7H5JJByy",
        "value": "apiKey",
        "required": true,
        "regex": "^(.{0,100})$",
        "regexErrorMessage": "Invalid Data Api Key"
      },
      {
        "type": "textInput",
        "label": "Account ID",
        "placeholder": "e.g: 6f59dd3f8a55ea850f53822cd32b29a7",
        "value": "accountId",
        "regex": "^(.{0,100})$",
        "regexErrorMessage": "Invalid Account ID",
        "required": true
      }
    ]
  },
  {
    "title": "Other Settings",
    "fields": [
      {
        "type": "textInput",
        "label": "Stream Name",
        "regex": "^(.{0,100})$",
        "regexErrorMessage": "Invalid Stream Name",
        "value": "stream",
        "default": "default"
      },
      {
        "type": "checkbox",
        "label": "Allow UID Sharing Across Multiple Domains",
        "value": "loadid",
        "default": false
      },
      {
        "type": "checkbox",
        "label": "Ensure Entity",
        "value": "blockload",
        "default": false
      }
    ]
  },
  {
    "title": "Native SDK",
    "fields": [
      {
        "type": "checkbox",
        "label": "Use native SDK to send events",
        "value": "useNativeSDK",
        "default": false
      }
    ]
  }
]