[
  {
    "title": "Connection Settings",
    "fields": [
      {
        "type": "textInput",
        "label": "Application ID",
        "value": "applicationId",
        "regex": "^(.{0,100})$",
        "regexErrorMessage": "Invalid Application ID",
        "required": true,
        "placeholder": "e.g: ABCDEFG"
      },
      {
        "type": "textInput",
        "label": "Client Key",
        "value": "clientKey",
        "regex": "^(.{0,100})$",
        "regexErrorMessage": "Invalid Client Key",
        "required": true,
        "placeholder": "e.g: ABCDEFG",
        "secret": true
      }
    ]
  },
  {
    "title": "Native SDK",
    "fields": [
      {
        "type": "checkbox",
        "label": "Initialize Native SDK to send automated events",
        "value": "useNativeSDK",
        "default": true
      },
      {
        "type": "checkbox",
        "label": "Use device-mode to send user generated events",
        "value": "useNativeSDKToSend",
        "default": false
      }
    ]
  },
  {
    "title": "Development Mode",
    "fields": [
      {
        "type": "checkbox",
        "label": "Use in Development Environment",
        "value": "isDevelop",
        "default": false
      }
    ]
  }
]