[
  {
    "title": "1. Connection Settings",
    "fields": [
      {
        "type": "textInput",
        "label": "Branch Key",
        "value": "branchKey",
        "regex": "^(.{0,100})$",
        "regexErrorMessage": "Invalid Branch Key",
        "required": true,
        "placeholder": "e.g: key_live_mfMJCn7WLNeXte2aZIDOeobdrvmbZpgD"
      }
    ]
  },
  {
    "title": "2. Native SDK",
    "fields": [
      {
        "type": "checkbox",
        "label": "Use device-mode to send events",
        "value": "useNativeSDK",
        "default": false
      }
    ]
  }
]